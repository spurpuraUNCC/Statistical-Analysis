    up_delta_mucin_mean=np.mean(up_delta_mucin[np.logical_not(np.isnan(up_delta_mucin))])
    up_delta_stp_mean=np.mean(up_delta_stp[np.logical_not(np.isnan(up_delta_stp))])
    up_delta_scp_mean=np.mean(up_delta_scp[np.logical_not(np.isnan(up_delta_scp))])
    up_delta_sherb_mean=np.mean(up_delta_sherb[np.logical_not(np.isnan(up_delta_sherb))])
