    up_pc_6km_shear_mean=np.mean(up_pc_6km_shear[np.isfinite(up_pc_6km_shear)])
    up_pc_sbcape_mean=np.mean(up_pc_sbcape[np.isfinite(up_pc_sbcape)])
    up_pc_mlcape_mean=np.mean(up_pc_mlcape[np.isfinite(up_pc_mlcape)])
    up_pc_mucape_mean=np.mean(up_pc_mucape[np.isfinite(up_pc_mucape)])
    up_pc_sbcin_mean=np.mean(up_pc_sbcin[np.isfinite(up_pc_sbcin)])
    up_pc_mlcin_mean=np.mean(up_pc_mlcin[np.isfinite(up_pc_mlcin)])
    up_pc_mucin_mean=np.mean(up_pc_mucin[np.isfinite(up_pc_mucin)])
    up_pc_stp_mean=np.mean(up_pc_stp[np.isfinite(up_pc_stp)])
    up_pc_scp_mean=np.mean(up_pc_scp[np.isfinite(up_pc_scp)])
    up_pc_sherb_mean=np.mean(up_pc_sherb[np.isfinite(up_pc_sherb)])
    up_pc_1km_lapse_mean=np.mean(up_pc_1km_lapse[np.isfinite(up_pc_1km_lapse)])
    up_pc_2km_lapse_mean=np.mean(up_pc_2km_lapse[np.isfinite(up_pc_2km_lapse)])
    up_pc_3km_lapse_mean=np.mean(up_pc_3km_lapse[np.isfinite(up_pc_3km_lapse)])
    up_pc_6km_lapse_mean=np.mean(up_pc_6km_lapse[np.isfinite(up_pc_6km_lapse)])
    up_pc_mid_rh_mean=np.mean(up_pc_mid_rh[np.isfinite(up_pc_mid_rh)])
    up_pc_sfc_theta_mean=np.mean(up_pc_sfc_theta[np.isfinite(up_pc_sfc_theta)])
    up_pc_ml_theta_mean=np.mean(up_pc_ml_theta[np.isfinite(up_pc_ml_theta)])
    up_pc_dcape_mean=np.mean(up_pc_dcape[np.isfinite(up_pc_dcape)])
    
    #Peak
    peak_pc_lcl_mean=np.mean(peak_pc_lcl[np.isfinite(peak_pc_lcl)])
