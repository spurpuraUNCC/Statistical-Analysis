    cross_up_peak_dcape_t,cross_up_peak_dcape_pvalue_t=stats.ttest_ind(cross_delta_up_peak_dcape[np.isfinite(
        cross_delta_up_peak_dcape)],non_delta_up_peak_dcape[np.isfinite(non_delta_up_peak_dcape)])


    #
    cross_peak_down_lcl_t,cross_peak_down_lcl_pvalue_t=stats.ttest_ind(cross_delta_peak_down_lcl[np.isfinite(
        cross_delta_peak_down_lcl)],non_delta_peak_down_lcl[np.isfinite(non_delta_peak_down_lcl)])
    cross_peak_down_lfc_t,cross_peak_down_lfc_pvalue_t=stats.ttest_ind(cross_delta_peak_down_lfc[np.isfinite(
        cross_delta_peak_down_lfc)],non_delta_peak_down_lfc[np.isfinite(non_delta_peak_down_lfc)])
