                                                   non_down_dcape[np.isfinite(non_down_dcape)]))


    ##Upstream vs downstream
    #Crossers
    cross_up_down_lcl_t,cross_up_down_lcl_pvalue_t=stats.ttest_ind(cross_delta_up_down_lcl[np.isfinite(
        cross_delta_up_down_lcl)],non_delta_up_down_lcl[np.isfinite(non_delta_up_down_lcl)])
    cross_up_down_lfc_t,cross_up_down_lfc_pvalue_t=stats.ttest_ind(cross_delta_up_down_lfc[np.isfinite(
        cross_delta_up_down_lfc)],non_delta_up_down_lfc[np.isfinite(non_delta_up_down_lfc)])
