                 str(up_pc_dcape_mean)+'\t'+str(up_dcape_ks)+'\t'+str(up_dcape_pvalue)+'\t'+str(up_dcape_t)+'\t'+
                  str(up_dcape_pvalue_t)+'\t'+'dcape:'+'\t'+str(cross_peak_dcape_mean)+'\t'+str(non_peak_dcape_mean)+'\t'+
                  str(peak_delta_dcape_mean)+'\t'+str(peak_pc_dcape_mean)+'\t'+str(peak_dcape_ks)+'\t'+str(peak_dcape_pvalue)+'\t'+
                  str(peak_dcape_t)+'\t'+str(peak_dcape_pvalue_t)+'\t'+'dcape:'+'\t'+str(cross_down_dcape_mean)+'\t'+
                  str(non_down_dcape_mean)+'\t'+str(down_delta_dcape_mean)+'\t'+str(down_pc_dcape_mean)+'\t'+
                  str(down_dcape_ks)+'\t'+str(down_dcape_pvalue)+'\t'+str(down_dcape_t)+'\t'+str(down_dcape_pvalue_t)+'\t'+
                  'dcape:'+'\t'+str(cross_delta_up_down_dcape_mean)+'\t'+
                  str(non_delta_up_down_dcape_mean)+'\t'+str(cross_pc_up_down_dcape_mean)+'\t'+
                  str(non_pc_up_down_dcape_mean)+'\t'+str(cross_up_down_dcape_ks)+'\t'+str(cross_up_down_dcape_pvalue)+'\t'+
                  str(cross_up_down_dcape_t)+'\t'+str(cross_up_down_dcape_pvalue_t)+'\t'+'dcape:'+'\t'+
                  str(cross_delta_up_peak_dcape_mean)+'\t'+str(non_delta_up_peak_dcape_mean)+'\t'+
                  str(cross_pc_up_peak_dcape_mean)+'\t'+str(non_pc_up_peak_dcape_mean)+'\t'+str(cross_up_peak_dcape_ks)+'\t'+
                  str(cross_up_peak_dcape_pvalue)+'\t'+str(cross_up_peak_dcape_t)+'\t'+str(cross_up_peak_dcape_pvalue_t)+'\t'+
                  'dcape:'+'\t'+str(cross_delta_peak_down_dcape_mean)+'\t'+str(non_delta_peak_down_dcape_mean)+'\t'+
                  str(cross_pc_peak_down_dcape_mean)+'\t'+str(non_pc_peak_down_dcape_mean)+'\t'+
                  str(cross_peak_down_dcape_ks)+'\t'+str(cross_peak_down_dcape_pvalue)+'\t'+str(cross_peak_down_dcape_t)+'\t'+
                  str(cross_peak_down_dcape_pvalue_t)+'\n')
                  
#read in the data
up_all=pd.read_csv(r'/Users/filepath.csv')
peak_all=pd.read_csv(r'/Users/filepath.csv')
down_all=pd.read_csv(r'/Users/filepath.csv')
outfile=open(r'/Users/filepath.csv','w')

#Call definition
ks_test(up_all,peak_all,down_all,outfile)
