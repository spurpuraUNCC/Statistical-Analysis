    ##Percent change:
    
    #Upstream
    up_pc_lcl_mean=np.mean(up_pc_lcl[np.isfinite(up_pc_lcl)])
    up_pc_lfc_mean=np.mean(up_pc_lfc[np.isfinite(up_pc_lfc)])
    up_pc_el_mean=np.mean(up_pc_el[np.isfinite(up_pc_el)])
    up_pc_h0_mean=np.mean(up_pc_h0[np.isfinite(up_pc_h0)])
    up_pc_h_neg20_mean=np.mean(up_pc_h_neg20[np.isfinite(up_pc_h_neg20)])
    up_pc_wbzh_mean=np.mean(up_pc_wbzh[np.isfinite(up_pc_wbzh)])
