        cross_delta_up_peak_mlcape)],non_delta_up_peak_mlcape[np.isfinite(non_delta_up_peak_mlcape)])
    cross_up_peak_mucape_t,cross_up_peak_mucape_pvalue_t=stats.ttest_ind(cross_delta_up_peak_mucape[np.isfinite(
        cross_delta_up_peak_mucape)],non_delta_up_peak_mucape[np.isfinite(non_delta_up_peak_mucape)])
    cross_up_peak_sbcin_t,cross_up_peak_sbcin_pvalue_t=stats.ttest_ind(cross_delta_up_peak_sbcin[np.isfinite(
        cross_delta_up_peak_sbcin)],non_delta_up_peak_sbcin[np.isfinite(non_delta_up_peak_sbcin)])
