        cross_delta_up_peak_mucin)],non_delta_up_peak_mucin[np.isfinite(non_delta_up_peak_mucin)])
    cross_up_peak_stp_t,cross_up_peak_stp_pvalue_t=stats.ttest_ind(cross_delta_up_peak_stp[np.isfinite(
        cross_delta_up_peak_stp)],non_delta_up_peak_stp[np.isfinite(non_delta_up_peak_stp)])
    cross_up_peak_scp_t,cross_up_peak_scp_pvalue_t=stats.ttest_ind(cross_delta_up_peak_scp[np.isfinite(
        cross_delta_up_peak_scp)],non_delta_up_peak_scp[np.isfinite(non_delta_up_peak_scp)])
