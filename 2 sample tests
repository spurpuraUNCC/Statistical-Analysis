    cross_down_h_neg20=(((np.array(df3[8:9])).flatten())[1:].astype('float64'))
    cross_down_wbzh=(((np.array(df3[9:10])).flatten())[1:].astype('float64'))
    cross_down_1km_srh=(((np.array(df3[10:11])).flatten())[1:].astype('float64'))
