        cross_delta_up_peak_lfc)],non_delta_up_peak_lfc[np.isfinite(non_delta_up_peak_lfc)])
    cross_up_peak_el_t,cross_up_peak_el_pvalue_t=stats.ttest_ind(cross_delta_up_peak_el[np.isfinite(
        cross_delta_up_peak_el)],non_delta_up_peak_el[np.isfinite(non_delta_up_peak_el)])
    cross_up_peak_h0_t,cross_up_peak_h0_pvalue_t=stats.ttest_ind(cross_delta_up_peak_h0[np.isfinite(
        cross_delta_up_peak_h0)],non_delta_up_peak_h0[np.isfinite(non_delta_up_peak_h0)])
    cross_up_peak_neg20_t,cross_up_peak_neg20_pvalue_t=stats.ttest_ind(cross_delta_up_peak_neg20[np.isfinite(
        cross_delta_up_peak_neg20)],non_delta_up_peak_neg20[np.isfinite(non_delta_up_peak_neg20)])
    cross_up_peak_wbzh_t,cross_up_peak_wbzh_pvalue_t=stats.ttest_ind(cross_delta_up_peak_wbzh[np.isfinite(
        cross_delta_up_peak_wbzh)],non_delta_up_peak_wbzh[np.isfinite(non_delta_up_peak_wbzh)])
    cross_up_peak_1km_srh_t,cross_up_peak_1km_srh_pvalue_t=stats.ttest_ind(cross_delta_up_peak_1km_srh[np.isfinite(
        cross_delta_up_peak_1km_srh)],non_delta_up_peak_1km_srh[np.isfinite(non_delta_up_peak_1km_srh)])
    cross_up_peak_3km_srh_t,cross_up_peak_3km_srh_pvalue_t=stats.ttest_ind(cross_delta_up_peak_3km_srh[np.isfinite(
        cross_delta_up_peak_3km_srh)],non_delta_up_peak_3km_srh[np.isfinite(non_delta_up_peak_3km_srh)])
    cross_up_peak_1km_shear_t,cross_up_peak_1km_shear_pvalue_t=stats.ttest_ind(cross_delta_up_peak_1km_shear[np.isfinite(
        cross_delta_up_peak_1km_shear)],non_delta_up_peak_1km_shear[np.isfinite(non_delta_up_peak_1km_shear)])
    cross_up_peak_3km_shear_t,cross_up_peak_3km_shear_pvalue_t=stats.ttest_ind(cross_delta_up_peak_3km_shear[np.isfinite(
        cross_delta_up_peak_3km_shear)],non_delta_up_peak_3km_shear[np.isfinite(non_delta_up_peak_3km_shear)])
