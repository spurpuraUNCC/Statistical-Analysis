        cross_delta_peak_down_1km_srh)],non_delta_peak_down_1km_srh[np.isfinite(non_delta_peak_down_1km_srh)])
    cross_peak_down_3km_srh_t,cross_peak_down_3km_srh_pvalue_t=stats.ttest_ind(cross_delta_peak_down_3km_srh[np.isfinite(
        cross_delta_peak_down_3km_srh)],non_delta_peak_down_3km_srh[np.isfinite(non_delta_peak_down_3km_srh)])
    cross_peak_down_1km_shear_t,cross_peak_down_1km_shear_pvalue_t=stats.ttest_ind(cross_delta_peak_down_1km_shear[np.isfinite(
        cross_delta_peak_down_1km_shear)],non_delta_peak_down_1km_shear[np.isfinite(non_delta_peak_down_1km_shear)])
    cross_peak_down_3km_shear_t,cross_peak_down_3km_shear_pvalue_t=stats.ttest_ind(cross_delta_peak_down_3km_shear[np.isfinite(
        cross_delta_peak_down_3km_shear)],non_delta_peak_down_3km_shear[np.isfinite(non_delta_peak_down_3km_shear)])
    cross_peak_down_6km_shear_t,cross_peak_down_6km_shear_pvalue_t=stats.ttest_ind(cross_delta_peak_down_6km_shear[np.isfinite(
        cross_delta_peak_down_6km_shear)],non_delta_peak_down_6km_shear[np.isfinite(non_delta_peak_down_6km_shear)])
    cross_peak_down_sbcape_t,cross_peak_down_sbcape_pvalue_t=stats.ttest_ind(cross_delta_peak_down_sbcape[np.isfinite(
        cross_delta_peak_down_sbcape)],non_delta_peak_down_sbcape[np.isfinite(non_delta_peak_down_sbcape)])
    cross_peak_down_mlcape_t,cross_peak_down_mlcape_pvalue_t=stats.ttest_ind(cross_delta_peak_down_mlcape[np.isfinite(
        cross_delta_peak_down_mlcape)],non_delta_peak_down_mlcape[np.isfinite(non_delta_peak_down_mlcape)])
    cross_peak_down_mucape_t,cross_peak_down_mucape_pvalue_t=stats.ttest_ind(cross_delta_peak_down_mucape[np.isfinite(
        cross_delta_peak_down_mucape)],non_delta_peak_down_mucape[np.isfinite(non_delta_peak_down_mucape)])
    cross_peak_down_sbcin_t,cross_peak_down_sbcin_pvalue_t=stats.ttest_ind(cross_delta_peak_down_sbcin[np.isfinite(
        cross_delta_peak_down_sbcin)],non_delta_peak_down_sbcin[np.isfinite(non_delta_peak_down_sbcin)])
    cross_peak_down_mlcin_t,cross_peak_down_mlcin_pvalue_t=stats.ttest_ind(cross_delta_peak_down_mlcin[np.isfinite(
        cross_delta_peak_down_mlcin)],non_delta_peak_down_mlcin[np.isfinite(non_delta_peak_down_mlcin)])
    cross_peak_down_mucin_t,cross_peak_down_mucin_pvalue_t=stats.ttest_ind(cross_delta_peak_down_mucin[np.isfinite(
        cross_delta_peak_down_mucin)],non_delta_peak_down_mucin[np.isfinite(non_delta_peak_down_mucin)])
    cross_peak_down_stp_t,cross_peak_down_stp_pvalue_t=stats.ttest_ind(cross_delta_peak_down_stp[np.isfinite(
        cross_delta_peak_down_stp)],non_delta_peak_down_stp[np.isfinite(non_delta_peak_down_stp)])
    cross_peak_down_scp_t,cross_peak_down_scp_pvalue_t=stats.ttest_ind(cross_delta_peak_down_scp[np.isfinite(
        cross_delta_peak_down_scp)],non_delta_peak_down_scp[np.isfinite(non_delta_peak_down_scp)])
    cross_peak_down_sherb_t,cross_peak_down_sherb_pvalue_t=stats.ttest_ind(cross_delta_peak_down_sherb[np.isfinite(
        cross_delta_peak_down_sherb)],non_delta_peak_down_sherb[np.isfinite(non_delta_peak_down_sherb)])
    cross_peak_down_1km_lapse_t,cross_peak_down_1km_lapse_pvalue_t=stats.ttest_ind(cross_delta_peak_down_1km_lapse[np.isfinite(
        cross_delta_peak_down_1km_lapse)],non_delta_peak_down_1km_lapse[np.isfinite(non_delta_peak_down_1km_lapse)])
    cross_peak_down_2km_lapse_t,cross_peak_down_2km_lapse_pvalue_t=stats.ttest_ind(cross_delta_peak_down_2km_lapse[np.isfinite(
        cross_delta_peak_down_2km_lapse)],non_delta_peak_down_2km_lapse[np.isfinite(non_delta_peak_down_2km_lapse)])
    cross_peak_down_3km_lapse_t,cross_peak_down_3km_lapse_pvalue_t=stats.ttest_ind(cross_delta_peak_down_3km_lapse[np.isfinite(
        cross_delta_peak_down_3km_lapse)],non_delta_peak_down_3km_lapse[np.isfinite(non_delta_peak_down_3km_lapse)])
    cross_peak_down_6km_lapse_t,cross_peak_down_6km_lapse_pvalue_t=stats.ttest_ind(cross_delta_peak_down_6km_lapse[np.isfinite(
        cross_delta_peak_down_6km_lapse)],non_delta_peak_down_6km_lapse[np.isfinite(non_delta_peak_down_6km_lapse)])
    cross_peak_down_mid_rh_t,cross_peak_down_mid_rh_pvalue_t=stats.ttest_ind(cross_delta_peak_down_mid_rh[np.isfinite(
        cross_delta_peak_down_mid_rh)],non_delta_peak_down_mid_rh[np.isfinite(non_delta_peak_down_mid_rh)])
    cross_peak_down_sfc_theta_t,cross_peak_down_sfc_theta_pvalue_t=stats.ttest_ind(cross_delta_peak_down_sfc_theta[np.isfinite(
        cross_delta_peak_down_sfc_theta)],non_delta_peak_down_sfc_theta[np.isfinite(non_delta_peak_down_sfc_theta)])
    cross_peak_down_ml_theta_t,cross_peak_down_ml_theta_pvalue_t=stats.ttest_ind(cross_delta_peak_down_ml_theta[np.isfinite(
        cross_delta_peak_down_ml_theta)],non_delta_peak_down_ml_theta[np.isfinite(non_delta_peak_down_ml_theta)])
    cross_peak_down_dcape_t,cross_peak_down_dcape_pvalue_t=stats.ttest_ind(cross_delta_peak_down_dcape[np.isfinite(
        cross_delta_peak_down_dcape)],non_delta_peak_down_dcape[np.isfinite(non_delta_peak_down_dcape)])
  
    
    
    outfile.write('Upstream'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'Peak'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+
                 'Downstream'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'Upstream vs. Downstream'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'
                  +'\t'+'\t'+'Upstream vs. Peak'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'Peak vs. Downstream'+'\t'+
                  '\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\t'+'\n')
    outfile.write('Parameter'+'\t'+'Mean Crosser'+'\t'+'Mean Non-crosser'+'\t'+'Mean Delta'+'\t'+'Mean Percent Change'+'\t'
                  +'KS Stat'+'\t'+'KS P-value'+'\t'+'T Stat'+'\t'+'T P-value'+'\t'+'Parameter'+'\t'+'Mean Crosser'+'\t'
                  +'Mean Non-crosser'+'\t'+'Mean Delta'+'\t'+'Mean Percent Change'+'\t'+'KS Stat'+'\t'+'P-value'+'\t'
                  +'T Stat'+'\t'+'T P-value'+'\t'+'Parameter'+'\t'+'Mean Crosser'+'\t'+'Mean Non-crosser'+'\t'
                  +'Mean Delta'+'\t'+'Mean Percent Change'+'\t'+'KS Stat'+'\t'+'P-value'+'\t'+'T Stat'+'\t'
                  +'T P-value'+'\t'+'Parameter'+'\t'+'Mean Crosser Delta'+'\t'+'Mean Non-crosser Delta'+'\t'
                  +'Mean Percent Change Crosser'+'\t'+'Mean Percent Change Non-crosser'+'\t'+'KS Stat'+'\t'+'P-value'+'\t'
                  +'T Stat'+'\t'+'T P-value'+'\t'+'Parameter'+'\t'+'Mean Crosser Delta'+'\t'+'Mean Non-crosser Delta'+'\t'
                  +'Mean Percent Change Crosser'+'\t'+'Mean Percent Change Non-crosser'+'\t'+'KS Stat'+'\t'+'P-value'+'\t'
                  +'T Stat'+'\t'+'T P-value'+'\t'+'Parameter'+'\t'+'Mean Crosser Delta'+'\t'+'Mean Non-crosser Delta'+'\t'
                  +'Mean Percent Change Crosser'+'\t'+'Mean Percent Change Non-crosser'+'\t'+'KS Stat'+'\t'+'P-value'+'\t'
                  +'T Stat'+'\t'+'T P-value'+'\t'+'\n')
   
    outfile.write('LCL:'+'\t'+str(cross_up_lcl_mean)+'\t'+str(non_up_lcl_mean)+'\t'+str(up_delta_lcl_mean)+'\t'+
                 str(up_pc_lcl_mean)+'\t'+str(up_lcl_ks)+'\t'+str(up_lcl_pvalue)+'\t'+str(up_lcl_t)+'\t'+
                  str(up_lcl_pvalue_t)+'\t'+'LCL:'+'\t'+str(cross_peak_lcl_mean)+'\t'+str(non_peak_lcl_mean)+'\t'+
                  str(peak_delta_lcl_mean)+'\t'+str(peak_pc_lcl_mean)+'\t'+str(peak_lcl_ks)+'\t'+str(peak_lcl_pvalue)+'\t'+
                  str(peak_lcl_t)+'\t'+str(peak_lcl_pvalue_t)+'\t'+'LCL:'+'\t'+str(cross_down_lcl_mean)+'\t'+
                  str(non_down_lcl_mean)+'\t'+str(down_delta_lcl_mean)+'\t'+str(down_pc_lcl_mean)+'\t'+
                  str(down_lcl_ks)+'\t'+str(down_lcl_pvalue)+'\t'+str(down_lcl_t)+'\t'+str(down_lcl_pvalue_t)+'\t'+
                  'LCL:'+'\t'+str(cross_delta_up_down_lcl_mean)+'\t'+
                  str(non_delta_up_down_lcl_mean)+'\t'+str(cross_pc_up_down_lcl_mean)+'\t'+
                  str(non_pc_up_down_lcl_mean)+'\t'+str(cross_up_down_lcl_ks)+'\t'+str(cross_up_down_lcl_pvalue)+'\t'+
                  str(cross_up_down_lcl_t)+'\t'+str(cross_up_down_lcl_pvalue_t)+'\t'+'LCL:'+'\t'+
                  str(cross_delta_up_peak_lcl_mean)+'\t'+str(non_delta_up_peak_lcl_mean)+'\t'+
                  str(cross_pc_up_peak_lcl_mean)+'\t'+str(non_pc_up_peak_lcl_mean)+'\t'+str(cross_up_peak_lcl_ks)+'\t'+
