        cross_delta_up_down_mlcape)],non_delta_up_down_mlcape[np.isfinite(non_delta_up_down_mlcape)])
    cross_up_down_mucape_t,cross_up_down_mucape_pvalue_t=stats.ttest_ind(cross_delta_up_down_mucape[np.isfinite(
        cross_delta_up_down_mucape)],non_delta_up_down_mucape[np.isfinite(non_delta_up_down_mucape)])
    cross_up_down_sbcin_t,cross_up_down_sbcin_pvalue_t=stats.ttest_ind(cross_delta_up_down_sbcin[np.isfinite(
        cross_delta_up_down_sbcin)],non_delta_up_down_sbcin[np.isfinite(non_delta_up_down_sbcin)])
    cross_up_down_mlcin_t,cross_up_down_mlcin_pvalue_t=stats.ttest_ind(cross_delta_up_down_mlcin[np.isfinite(
        cross_delta_up_down_mlcin)],non_delta_up_down_mlcin[np.isfinite(non_delta_up_down_mlcin)])
