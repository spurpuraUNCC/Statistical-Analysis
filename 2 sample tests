    down_pc_stp_mean=np.mean(down_pc_stp[np.isfinite(down_pc_stp)])
    down_pc_scp_mean=np.mean(down_pc_scp[np.isfinite(down_pc_scp)])
    down_pc_sherb_mean=np.mean(down_pc_sherb[np.isfinite(down_pc_sherb)])
    down_pc_1km_lapse_mean=np.mean(down_pc_1km_lapse[np.isfinite(down_pc_1km_lapse)])
