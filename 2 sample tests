    non_up_h_neg20=(((np.array(df1[43:44])).flatten())[1:].astype('float64'))
    non_up_wbzh=(((np.array(df1[45:46])).flatten())[1:].astype('float64'))
    non_up_1km_srh=(((np.array(df1[46:47])).flatten())[1:].astype('float64'))
    non_up_1km_shear=(((np.array(df1[47:48])).flatten())[1:].astype('float64'))
    non_up_3km_srh=(((np.array(df1[48:49])).flatten())[1:].astype('float64'))
    non_up_3km_shear=(((np.array(df1[49:50])).flatten())[1:].astype('float64'))
    non_up_6km_shear=(((np.array(df1[50:51])).flatten())[1:].astype('float64'))
    non_up_sbcape=(((np.array(df1[51:52])).flatten())[1:].astype('float64'))
    non_up_sbcin=(((np.array(df1[52:53])).flatten())[1:].astype('float64'))
    non_up_mlcape=(((np.array(df1[53:54])).flatten())[1:].astype('float64'))
    non_up_mlcin=(((np.array(df1[54:55])).flatten())[1:].astype('float64'))
    non_up_mucape=(((np.array(df1[55:56])).flatten())[1:].astype('float64'))
    non_up_mucin=(((np.array(df1[56:57])).flatten())[1:].astype('float64'))
    non_up_stp=(((np.array(df1[57:58])).flatten())[1:].astype('float64'))
    non_up_scp=(((np.array(df1[58:59])).flatten())[1:].astype('float64'))
    non_up_1km_lapse=(((np.array(df1[60:61])).flatten())[1:].astype('float64'))
    non_up_2km_lapse=(((np.array(df1[61:62])).flatten())[1:].astype('float64'))
    non_up_3km_lapse=(((np.array(df1[62:63])).flatten())[1:].astype('float64'))
    non_up_6km_lapse=(((np.array(df1[63:64])).flatten())[1:].astype('float64'))
    non_up_mid_lapse=(((np.array(df1[64:65])).flatten())[1:].astype('float64'))
    non_up_mid_rh=(((np.array(df1[65:66])).flatten())[1:].astype('float64'))
    non_up_sherb=(((np.array(df1[66:67])).flatten())[1:].astype('float64'))
    non_up_sfc_theta=(((np.array(df1[67:68])).flatten())[1:].astype('float64'))
    non_up_ml_theta=(((np.array(df1[68:69])).flatten())[1:].astype('float64'))
    non_up_dcape=(((np.array(df1[69:70])).flatten())[1:].astype('float64'))



    #Peak
    non_peak_sfc_p=(((np.array(df2[37:38])).flatten())[1:].astype('float64'))
    non_peak_lcl=(((np.array(df2[39:40])).flatten())[1:].astype('float64'))
    non_peak_lfc=(((np.array(df2[40:41])).flatten())[1:].astype('float64'))
    non_peak_el=(((np.array(df2[41:42])).flatten())[1:].astype('float64'))
    non_peak_h0=(((np.array(df2[42:43])).flatten())[1:].astype('float64'))
    non_peak_h_neg20=(((np.array(df2[43:44])).flatten())[1:].astype('float64'))
    non_peak_wbzh=(((np.array(df2[45:46])).flatten())[1:].astype('float64'))
    non_peak_1km_srh=(((np.array(df2[46:47])).flatten())[1:].astype('float64'))
    non_peak_1km_shear=(((np.array(df2[47:48])).flatten())[1:].astype('float64'))
    non_peak_3km_srh=(((np.array(df2[48:49])).flatten())[1:].astype('float64'))
    non_peak_3km_shear=(((np.array(df2[49:50])).flatten())[1:].astype('float64'))
    non_peak_6km_shear=(((np.array(df2[50:51])).flatten())[1:].astype('float64'))
    non_peak_sbcape=(((np.array(df2[51:52])).flatten())[1:].astype('float64'))
    non_peak_sbcin=(((np.array(df2[52:53])).flatten())[1:].astype('float64'))
    non_peak_mlcape=(((np.array(df2[53:54])).flatten())[1:].astype('float64'))
    non_peak_mlcin=(((np.array(df2[54:55])).flatten())[1:].astype('float64'))
    non_peak_mucape=(((np.array(df2[55:56])).flatten())[1:].astype('float64'))
    non_peak_mucin=(((np.array(df2[56:57])).flatten())[1:].astype('float64'))
    non_peak_stp=(((np.array(df2[57:58])).flatten())[1:].astype('float64'))
    non_peak_scp=(((np.array(df2[58:59])).flatten())[1:].astype('float64'))
    non_peak_1km_lapse=(((np.array(df2[60:61])).flatten())[1:].astype('float64'))
    non_peak_2km_lapse=(((np.array(df2[61:62])).flatten())[1:].astype('float64'))
    non_peak_3km_lapse=(((np.array(df2[62:63])).flatten())[1:].astype('float64'))
    non_peak_6km_lapse=(((np.array(df2[63:64])).flatten())[1:].astype('float64'))
    non_peak_mid_lapse=(((np.array(df2[64:65])).flatten())[1:].astype('float64'))
    non_peak_mid_rh=(((np.array(df2[65:66])).flatten())[1:].astype('float64'))
    non_peak_sherb=(((np.array(df2[66:67])).flatten())[1:].astype('float64'))
    non_peak_sfc_theta=(((np.array(df2[67:68])).flatten())[1:].astype('float64'))
    non_peak_ml_theta=(((np.array(df2[68:69])).flatten())[1:].astype('float64'))
    non_peak_dcape=(((np.array(df2[69:70])).flatten())[1:].astype('float64'))




    #Down
    non_down_sfc_p=(((np.array(df3[37:38])).flatten())[1:].astype('float64'))
    non_down_lcl=(((np.array(df3[39:40])).flatten())[1:].astype('float64'))
    non_down_lfc=(((np.array(df3[40:41])).flatten())[1:].astype('float64'))
    non_down_el=(((np.array(df3[41:42])).flatten())[1:].astype('float64'))
    non_down_h0=(((np.array(df3[42:43])).flatten())[1:].astype('float64'))
    non_down_h_neg20=(((np.array(df3[43:44])).flatten())[1:].astype('float64'))
    non_down_wbzh=(((np.array(df3[45:46])).flatten())[1:].astype('float64'))
    non_down_1km_srh=(((np.array(df3[46:47])).flatten())[1:].astype('float64'))
    non_down_1km_shear=(((np.array(df3[47:48])).flatten())[1:].astype('float64'))
    non_down_3km_srh=(((np.array(df3[48:49])).flatten())[1:].astype('float64'))
    non_down_3km_shear=(((np.array(df3[49:50])).flatten())[1:].astype('float64'))
    non_down_6km_shear=(((np.array(df3[50:51])).flatten())[1:].astype('float64'))
    non_down_sbcape=(((np.array(df3[51:52])).flatten())[1:].astype('float64'))
    non_down_sbcin=(((np.array(df3[52:53])).flatten())[1:].astype('float64'))
    non_down_mlcape=(((np.array(df3[53:54])).flatten())[1:].astype('float64'))
    non_down_mlcin=(((np.array(df3[54:55])).flatten())[1:].astype('float64'))
    non_down_mucape=(((np.array(df3[55:56])).flatten())[1:].astype('float64'))
    non_down_mucin=(((np.array(df3[56:57])).flatten())[1:].astype('float64'))
    non_down_stp=(((np.array(df3[57:58])).flatten())[1:].astype('float64'))
    non_down_scp=(((np.array(df3[58:59])).flatten())[1:].astype('float64'))
    non_down_1km_lapse=(((np.array(df3[60:61])).flatten())[1:].astype('float64'))
    non_down_2km_lapse=(((np.array(df3[61:62])).flatten())[1:].astype('float64'))
    non_down_3km_lapse=(((np.array(df3[62:63])).flatten())[1:].astype('float64'))
    non_down_6km_lapse=(((np.array(df3[63:64])).flatten())[1:].astype('float64'))
    non_down_mid_lapse=(((np.array(df3[64:65])).flatten())[1:].astype('float64'))
    non_down_mid_rh=(((np.array(df3[65:66])).flatten())[1:].astype('float64'))
    non_down_sherb=(((np.array(df3[66:67])).flatten())[1:].astype('float64'))
    non_down_sfc_theta=(((np.array(df3[67:68])).flatten())[1:].astype('float64'))
    non_down_ml_theta=(((np.array(df3[68:69])).flatten())[1:].astype('float64'))
    non_down_dcape=(((np.array(df3[69:70])).flatten())[1:].astype('float64'))



    
#Creates arrays to calculate the deltas

    ##Upstream:
    up_delta_lcl=non_up_lcl-cross_up_lcl
    up_delta_lfc=non_up_lfc-cross_up_lfc
    up_delta_el=non_up_el-cross_up_el
    up_delta_h0=non_up_h0-cross_up_h0
    up_delta_h_neg20=non_up_h_neg20-cross_up_h_neg20
    up_delta_wbzh=non_up_wbzh-cross_up_wbzh
    up_delta_1km_srh=non_up_1km_srh-cross_up_1km_srh
    up_delta_1km_shear=non_up_1km_shear-cross_up_1km_shear
    up_delta_3km_srh=non_up_3km_srh-cross_up_3km_srh
    up_delta_3km_shear=non_up_3km_shear-cross_up_3km_shear
    up_delta_6km_shear=non_up_6km_shear-cross_up_6km_shear
    up_delta_sbcape=non_up_sbcape-cross_up_sbcape
    up_delta_sbcin=non_up_sbcin-cross_up_sbcin
    up_delta_mlcape=non_up_mlcape-cross_up_mlcape
    up_delta_mlcin=non_up_mlcin-cross_up_mlcin
    up_delta_mucape=non_up_mucape-cross_up_mucape
    up_delta_mucin=non_up_mucin-cross_up_mucin
    up_delta_stp=non_up_stp-cross_up_stp
    up_delta_scp=non_up_scp-cross_up_scp
    up_delta_sherb=non_up_sherb-cross_up_sherb
    up_delta_1km_lapse=non_up_1km_lapse-cross_up_1km_lapse
    up_delta_2km_lapse=non_up_2km_lapse-cross_up_2km_lapse
    up_delta_3km_lapse=non_up_3km_lapse-cross_up_3km_lapse
    up_delta_6km_lapse=non_up_6km_lapse-cross_up_6km_lapse
    up_delta_mid_rh=non_up_mid_rh-cross_up_mid_rh
    up_delta_sfc_theta=non_up_sfc_theta-cross_up_sfc_theta
    up_delta_ml_theta=non_up_ml_theta-cross_up_ml_theta
    up_delta_dcape=non_up_dcape-cross_up_dcape
    
    ##Peak:
    peak_delta_lcl=non_peak_lcl-cross_peak_lcl
    peak_delta_lfc=non_peak_lfc-cross_peak_lfc
    peak_delta_el=non_peak_el-cross_peak_el
    peak_delta_h0=non_peak_h0-cross_peak_h0
    peak_delta_h_neg20=non_peak_h_neg20-cross_peak_h_neg20
    peak_delta_wbzh=non_peak_wbzh-cross_peak_wbzh
    peak_delta_wbzh=non_peak_wbzh-cross_peak_wbzh
    peak_delta_1km_srh=non_peak_1km_srh-cross_peak_1km_srh
    peak_delta_1km_shear=non_peak_1km_shear-cross_peak_1km_shear
    peak_delta_3km_srh=non_peak_3km_srh-cross_peak_3km_srh
    peak_delta_3km_shear=non_peak_3km_shear-cross_peak_3km_shear
    peak_delta_6km_shear=non_peak_6km_shear-cross_peak_6km_shear
    peak_delta_sbcape=non_peak_sbcape-cross_peak_sbcape
    peak_delta_sbcin=non_peak_sbcin-cross_peak_sbcin
    peak_delta_mlcape=non_peak_mlcape-cross_peak_mlcape
    peak_delta_mlcin=non_peak_mlcin-cross_peak_mlcin
    peak_delta_mucape=non_peak_mucape-cross_peak_mucape
    peak_delta_mucin=non_peak_mucin-cross_peak_mucin
    peak_delta_stp=non_peak_stp-cross_peak_stp
    peak_delta_scp=non_peak_scp-cross_peak_scp
    peak_delta_sherb=non_peak_sherb-cross_peak_sherb
    peak_delta_1km_lapse=non_peak_1km_lapse-cross_peak_1km_lapse
    peak_delta_2km_lapse=non_peak_2km_lapse-cross_peak_2km_lapse
    peak_delta_3km_lapse=non_peak_3km_lapse-cross_peak_3km_lapse
    peak_delta_6km_lapse=non_peak_6km_lapse-cross_peak_6km_lapse
    peak_delta_mid_rh=non_peak_mid_rh-cross_peak_mid_rh
    peak_delta_sfc_theta=non_peak_sfc_theta-cross_peak_sfc_theta
    peak_delta_ml_theta=non_peak_ml_theta-cross_peak_ml_theta
    peak_delta_dcape=non_peak_dcape-cross_peak_dcape

    ##Downstream:
    down_delta_lcl=non_down_lcl-cross_down_lcl
    down_delta_lfc=non_down_lfc-cross_down_lfc
    down_delta_el=non_down_el-cross_down_el
    down_delta_h0=non_down_h0-cross_down_h0
    down_delta_h_neg20=non_down_h_neg20-cross_down_h_neg20
    down_delta_wbzh=non_down_wbzh-cross_down_wbzh
    down_delta_wbzh=non_down_wbzh-cross_down_wbzh
    down_delta_1km_srh=non_down_1km_srh-cross_down_1km_srh
    down_delta_1km_shear=non_down_1km_shear-cross_down_1km_shear
    down_delta_3km_srh=non_down_3km_srh-cross_down_3km_srh
    down_delta_3km_shear=non_down_3km_shear-cross_down_3km_shear
    down_delta_6km_shear=non_down_6km_shear-cross_down_6km_shear
    down_delta_sbcape=non_down_sbcape-cross_down_sbcape
    down_delta_sbcin=non_down_sbcin-cross_down_sbcin
    down_delta_mlcape=non_down_mlcape-cross_down_mlcape
    down_delta_mlcin=non_down_mlcin-cross_down_mlcin
    down_delta_mucape=non_down_mucape-cross_down_mucape
    down_delta_mucin=non_down_mucin-cross_down_mucin
    down_delta_stp=non_down_stp-cross_down_stp
    down_delta_scp=non_down_scp-cross_down_scp
    down_delta_sherb=non_down_sherb-cross_down_sherb
    down_delta_1km_lapse=non_down_1km_lapse-cross_down_1km_lapse
    down_delta_2km_lapse=non_down_2km_lapse-cross_down_2km_lapse
    down_delta_3km_lapse=non_down_3km_lapse-cross_down_3km_lapse
    down_delta_6km_lapse=non_down_6km_lapse-cross_down_6km_lapse
    down_delta_mid_rh=non_down_mid_rh-cross_down_mid_rh
    down_delta_sfc_theta=non_down_sfc_theta-cross_down_sfc_theta
    down_delta_ml_theta=non_down_ml_theta-cross_down_ml_theta
    down_delta_dcape=non_down_dcape-cross_down_dcape
    
    ##Upstream and Downstream differences:
    #Crossers
    cross_delta_up_down_lcl=cross_down_lcl-cross_up_lcl
    cross_delta_up_down_lfc=cross_down_lfc-cross_up_lfc
    cross_delta_up_down_el=cross_down_el-cross_up_el
    cross_delta_up_down_h0=cross_down_h0-cross_up_h0
    cross_delta_up_down_neg20=cross_down_h_neg20-cross_up_h_neg20
    cross_delta_up_down_wbzh=cross_down_wbzh-cross_up_wbzh
    cross_delta_up_down_1km_srh=cross_down_1km_srh-cross_up_1km_srh
    cross_delta_up_down_1km_shear=cross_down_1km_shear-cross_up_1km_shear
    cross_delta_up_down_3km_srh=cross_down_3km_srh-cross_up_3km_srh
    cross_delta_up_down_3km_shear=cross_down_3km_shear-cross_up_3km_shear
    cross_delta_up_down_6km_shear=cross_down_6km_shear-cross_up_6km_shear
    cross_delta_up_down_sbcape=cross_down_sbcape-cross_up_sbcape
    cross_delta_up_down_sbcin=cross_down_sbcin-cross_up_sbcin
    cross_delta_up_down_mlcape=cross_down_mlcape-cross_up_mlcape
    cross_delta_up_down_mlcin=cross_down_mlcin-cross_up_mlcin
    cross_delta_up_down_mucape=cross_down_mucape-cross_up_mucape
    cross_delta_up_down_mucin=cross_down_mucin-cross_up_mucin
    cross_delta_up_down_stp=cross_down_stp-cross_up_stp
    cross_delta_up_down_scp=cross_down_scp-cross_up_scp
    cross_delta_up_down_sherb=cross_down_sherb-cross_up_sherb
    cross_delta_up_down_1km_lapse=cross_down_1km_lapse-cross_up_1km_lapse
    cross_delta_up_down_2km_lapse=cross_down_2km_lapse-cross_up_2km_lapse
    cross_delta_up_down_3km_lapse=cross_down_3km_lapse-cross_up_3km_lapse
    cross_delta_up_down_6km_lapse=cross_down_6km_lapse-cross_up_6km_lapse
    cross_delta_up_down_mid_rh=cross_down_mid_rh-cross_up_mid_rh
    cross_delta_up_down_sfc_theta=cross_down_sfc_theta-cross_up_sfc_theta
    cross_delta_up_down_ml_theta=cross_down_ml_theta-cross_up_ml_theta
    cross_delta_up_down_dcape=cross_down_dcape-cross_up_dcape
    #Non-crossers
    non_delta_up_down_lcl=non_down_lcl-non_up_lcl
    non_delta_up_down_lfc=non_down_lfc-non_up_lfc
    non_delta_up_down_el=non_down_el-non_up_el
    non_delta_up_down_h0=non_down_h0-non_up_h0
    non_delta_up_down_neg20=non_down_h_neg20-non_up_h_neg20
    non_delta_up_down_wbzh=non_down_wbzh-non_up_wbzh
    non_delta_up_down_1km_srh=non_down_1km_srh-non_up_1km_srh
    non_delta_up_down_1km_shear=non_down_1km_shear-non_up_1km_shear
    non_delta_up_down_3km_srh=non_down_3km_srh-non_up_3km_srh
    non_delta_up_down_3km_shear=non_down_3km_shear-non_up_3km_shear
    non_delta_up_down_6km_shear=non_down_6km_shear-non_up_6km_shear
    non_delta_up_down_sbcape=non_down_sbcape-non_up_sbcape
    non_delta_up_down_sbcin=non_down_sbcin-non_up_sbcin
    non_delta_up_down_mlcape=non_down_mlcape-non_up_mlcape
    non_delta_up_down_mlcin=non_down_mlcin-non_up_mlcin
    non_delta_up_down_mucape=non_down_mucape-non_up_mucape
    non_delta_up_down_mucin=non_down_mucin-non_up_mucin
    non_delta_up_down_stp=non_down_stp-non_up_stp
    non_delta_up_down_scp=non_down_scp-non_up_scp
    non_delta_up_down_sherb=non_down_sherb-non_up_sherb
    non_delta_up_down_1km_lapse=non_down_1km_lapse-non_up_1km_lapse
    non_delta_up_down_2km_lapse=non_down_2km_lapse-non_up_2km_lapse
    non_delta_up_down_3km_lapse=non_down_3km_lapse-non_up_3km_lapse
    non_delta_up_down_6km_lapse=non_down_6km_lapse-non_up_6km_lapse
    non_delta_up_down_mid_rh=non_down_mid_rh-non_up_mid_rh
    non_delta_up_down_sfc_theta=non_down_sfc_theta-non_up_sfc_theta
    non_delta_up_down_ml_theta=non_down_ml_theta-non_up_ml_theta
    non_delta_up_down_dcape=non_down_dcape-non_up_dcape

    ##Upstream and Peak differences:
    #Crossers
    cross_delta_up_peak_lcl=cross_peak_lcl-cross_up_lcl
    cross_delta_up_peak_lfc=cross_peak_lfc-cross_up_lfc
    cross_delta_up_peak_el=cross_peak_el-cross_up_el
    cross_delta_up_peak_h0=cross_peak_h0-cross_up_h0
    cross_delta_up_peak_neg20=cross_peak_h_neg20-cross_up_h_neg20
    cross_delta_up_peak_wbzh=cross_peak_wbzh-cross_up_wbzh
    cross_delta_up_peak_1km_srh=cross_peak_1km_srh-cross_up_1km_srh
    cross_delta_up_peak_1km_shear=cross_peak_1km_shear-cross_up_1km_shear
    cross_delta_up_peak_3km_srh=cross_peak_3km_srh-cross_up_3km_srh
    cross_delta_up_peak_3km_shear=cross_peak_3km_shear-cross_up_3km_shear
    cross_delta_up_peak_6km_shear=cross_peak_6km_shear-cross_up_6km_shear
    cross_delta_up_peak_sbcape=cross_peak_sbcape-cross_up_sbcape
    cross_delta_up_peak_sbcin=cross_peak_sbcin-cross_up_sbcin
    cross_delta_up_peak_mlcape=cross_peak_mlcape-cross_up_mlcape
    cross_delta_up_peak_mlcin=cross_peak_mlcin-cross_up_mlcin
    cross_delta_up_peak_mucape=cross_peak_mucape-cross_up_mucape
    cross_delta_up_peak_mucin=cross_peak_mucin-cross_up_mucin
    cross_delta_up_peak_stp=cross_peak_stp-cross_up_stp
    cross_delta_up_peak_scp=cross_peak_scp-cross_up_scp
    cross_delta_up_peak_sherb=cross_peak_sherb-cross_up_sherb
    cross_delta_up_peak_1km_lapse=cross_peak_1km_lapse-cross_up_1km_lapse
    cross_delta_up_peak_2km_lapse=cross_peak_2km_lapse-cross_up_2km_lapse
    cross_delta_up_peak_3km_lapse=cross_peak_3km_lapse-cross_up_3km_lapse
    cross_delta_up_peak_6km_lapse=cross_peak_6km_lapse-cross_up_6km_lapse
    cross_delta_up_peak_mid_rh=cross_peak_mid_rh-cross_up_mid_rh
    cross_delta_up_peak_sfc_theta=cross_peak_sfc_theta-cross_up_sfc_theta
    cross_delta_up_peak_ml_theta=cross_peak_ml_theta-cross_up_ml_theta
    cross_delta_up_peak_dcape=cross_peak_dcape-cross_up_dcape
    #Non-crossers
    non_delta_up_peak_lcl=non_peak_lcl-non_up_lcl
    non_delta_up_peak_lfc=non_peak_lfc-non_up_lfc
    non_delta_up_peak_el=non_peak_el-non_up_el
    non_delta_up_peak_h0=non_peak_h0-non_up_h0
    non_delta_up_peak_neg20=non_peak_h_neg20-non_up_h_neg20
    non_delta_up_peak_wbzh=non_peak_wbzh-non_up_wbzh
    non_delta_up_peak_1km_srh=non_peak_1km_srh-non_up_1km_srh
    non_delta_up_peak_1km_shear=non_peak_1km_shear-non_up_1km_shear
    non_delta_up_peak_3km_srh=non_peak_3km_srh-non_up_3km_srh
    non_delta_up_peak_3km_shear=non_peak_3km_shear-non_up_3km_shear
    non_delta_up_peak_6km_shear=non_peak_6km_shear-non_up_6km_shear
    non_delta_up_peak_sbcape=non_peak_sbcape-non_up_sbcape
    non_delta_up_peak_sbcin=non_peak_sbcin-non_up_sbcin
    non_delta_up_peak_mlcape=non_peak_mlcape-non_up_mlcape
    non_delta_up_peak_mlcin=non_peak_mlcin-non_up_mlcin
    non_delta_up_peak_mucape=non_peak_mucape-non_up_mucape
    non_delta_up_peak_mucin=non_peak_mucin-non_up_mucin
    non_delta_up_peak_stp=non_peak_stp-non_up_stp
    non_delta_up_peak_scp=non_peak_scp-non_up_scp
    non_delta_up_peak_sherb=non_peak_sherb-non_up_sherb
    non_delta_up_peak_1km_lapse=non_peak_1km_lapse-non_up_1km_lapse
    non_delta_up_peak_2km_lapse=non_peak_2km_lapse-non_up_2km_lapse
    non_delta_up_peak_3km_lapse=non_peak_3km_lapse-non_up_3km_lapse
    non_delta_up_peak_6km_lapse=non_peak_6km_lapse-non_up_6km_lapse
    non_delta_up_peak_mid_rh=non_peak_mid_rh-non_up_mid_rh
    non_delta_up_peak_sfc_theta=non_peak_sfc_theta-non_up_sfc_theta
    non_delta_up_peak_ml_theta=non_peak_ml_theta-non_up_ml_theta
    non_delta_up_peak_dcape=non_peak_dcape-non_up_dcape
    
    ##Peak and Downstream differences:
    #Crossers
    cross_delta_peak_down_lcl=cross_down_lcl-cross_peak_lcl
    cross_delta_peak_down_lfc=cross_down_lfc-cross_peak_lfc
    cross_delta_peak_down_el=cross_down_el-cross_peak_el
    cross_delta_peak_down_h0=cross_down_h0-cross_peak_h0
    cross_delta_peak_down_neg20=cross_down_h_neg20-cross_peak_h_neg20
    cross_delta_peak_down_wbzh=cross_down_wbzh-cross_peak_wbzh
    cross_delta_peak_down_1km_srh=cross_down_1km_srh-cross_peak_1km_srh
    cross_delta_peak_down_1km_shear=cross_down_1km_shear-cross_peak_1km_shear
    cross_delta_peak_down_3km_srh=cross_down_3km_srh-cross_peak_3km_srh
    cross_delta_peak_down_3km_shear=cross_down_3km_shear-cross_peak_3km_shear
    cross_delta_peak_down_6km_shear=cross_down_6km_shear-cross_peak_6km_shear
    cross_delta_peak_down_sbcape=cross_down_sbcape-cross_peak_sbcape
    cross_delta_peak_down_sbcin=cross_down_sbcin-cross_peak_sbcin
    cross_delta_peak_down_mlcape=cross_down_mlcape-cross_peak_mlcape
    cross_delta_peak_down_mlcin=cross_down_mlcin-cross_peak_mlcin
    cross_delta_peak_down_mucape=cross_down_mucape-cross_peak_mucape
    cross_delta_peak_down_mucin=cross_down_mucin-cross_peak_mucin
    cross_delta_peak_down_stp=cross_down_stp-cross_peak_stp
    cross_delta_peak_down_scp=cross_down_scp-cross_peak_scp
    cross_delta_peak_down_sherb=cross_down_sherb-cross_peak_sherb
    cross_delta_peak_down_1km_lapse=cross_down_1km_lapse-cross_peak_1km_lapse
    cross_delta_peak_down_2km_lapse=cross_down_2km_lapse-cross_peak_2km_lapse
    cross_delta_peak_down_3km_lapse=cross_down_3km_lapse-cross_peak_3km_lapse
    cross_delta_peak_down_6km_lapse=cross_down_6km_lapse-cross_peak_6km_lapse
    cross_delta_peak_down_mid_rh=cross_down_mid_rh-cross_peak_mid_rh
    cross_delta_peak_down_sfc_theta=cross_down_sfc_theta-cross_peak_sfc_theta
    cross_delta_peak_down_ml_theta=cross_down_ml_theta-cross_peak_ml_theta
    cross_delta_peak_down_dcape=cross_down_dcape-cross_peak_dcape
    #Non-crossers
    non_delta_peak_down_lcl=non_down_lcl-non_peak_lcl
    non_delta_peak_down_lfc=non_down_lfc-non_peak_lfc
    non_delta_peak_down_el=non_down_el-non_peak_el
    non_delta_peak_down_h0=non_down_h0-non_peak_h0
    non_delta_peak_down_neg20=non_down_h_neg20-non_peak_h_neg20
    non_delta_peak_down_wbzh=non_down_wbzh-non_peak_wbzh
    non_delta_peak_down_1km_srh=non_down_1km_srh-non_peak_1km_srh
    non_delta_peak_down_1km_shear=non_down_1km_shear-non_peak_1km_shear
    non_delta_peak_down_3km_srh=non_down_3km_srh-non_peak_3km_srh
    non_delta_peak_down_3km_shear=non_down_3km_shear-non_peak_3km_shear
    non_delta_peak_down_6km_shear=non_down_6km_shear-non_peak_6km_shear
    non_delta_peak_down_sbcape=non_down_sbcape-non_peak_sbcape
    non_delta_peak_down_sbcin=non_down_sbcin-non_peak_sbcin
    non_delta_peak_down_mlcape=non_down_mlcape-non_peak_mlcape
    non_delta_peak_down_mlcin=non_down_mlcin-non_peak_mlcin
    non_delta_peak_down_mucape=non_down_mucape-non_peak_mucape
    non_delta_peak_down_mucin=non_down_mucin-non_peak_mucin
    non_delta_peak_down_stp=non_down_stp-non_peak_stp
    non_delta_peak_down_scp=non_down_scp-non_peak_scp
    non_delta_peak_down_sherb=non_down_sherb-non_peak_sherb
    non_delta_peak_down_1km_lapse=non_down_1km_lapse-non_peak_1km_lapse
    non_delta_peak_down_2km_lapse=non_down_2km_lapse-non_peak_2km_lapse
    non_delta_peak_down_3km_lapse=non_down_3km_lapse-non_peak_3km_lapse
    non_delta_peak_down_6km_lapse=non_down_6km_lapse-non_peak_6km_lapse
    non_delta_peak_down_mid_rh=non_down_mid_rh-non_peak_mid_rh
    non_delta_peak_down_sfc_theta=non_down_sfc_theta-non_peak_sfc_theta
    non_delta_peak_down_ml_theta=non_down_ml_theta-non_peak_ml_theta
    non_delta_peak_down_dcape=non_down_dcape-non_peak_dcape
    
 
    
#Create arrays to calulcate percent change 
    
    #Upstream
    up_pc_lcl=(abs(up_delta_lcl)/abs(cross_up_lcl))*100
    up_pc_lfc=(abs(up_delta_lfc)/abs(cross_up_lfc))*100
    up_pc_el=(abs(up_delta_el)/abs(cross_up_el))*100
    up_pc_h0=(abs(up_delta_h0)/abs(cross_up_h0))*100
    up_pc_h0=(abs(up_delta_h0)/abs(cross_up_h0))*100
    up_pc_h_neg20=(abs(up_delta_h_neg20)/abs(cross_up_h_neg20))*100
    up_pc_wbzh=(abs(up_delta_wbzh)/abs(cross_up_wbzh))*100
    up_pc_wbzh=(abs(up_delta_wbzh)/abs(cross_up_wbzh))*100
    up_pc_1km_srh=(abs(up_delta_1km_srh)/abs(cross_up_1km_srh))*100
    up_pc_1km_shear=(abs(up_delta_1km_shear)/abs(cross_up_1km_shear))*100
    up_pc_3km_srh=(abs(up_delta_3km_srh)/abs(cross_up_3km_srh))*100
    up_pc_3km_shear=(abs(up_delta_3km_shear)/abs(cross_up_3km_shear))*100
    up_pc_6km_shear=(abs(up_delta_6km_shear)/abs(cross_up_6km_shear))*100
    up_pc_sbcape=(abs(up_delta_sbcape)/abs(cross_up_sbcape))*100
    up_pc_sbcin=(abs(up_delta_sbcin)/abs(cross_up_sbcin))*100
    up_pc_mlcape=(up_delta_mlcape/cross_up_mlcape)*100
    up_pc_mlcin=(up_delta_mlcin/cross_up_mlcin)*100
    up_pc_mucape=(up_delta_mucape/cross_up_mucape)*100
    up_pc_mucin=(up_delta_mucin/cross_up_mucin)*100
    up_pc_stp=(up_delta_stp/cross_up_stp)*100
    up_pc_scp=(up_delta_scp/cross_up_scp)*100
    up_pc_sherb=(up_delta_sherb/cross_up_sherb)*100
    up_pc_1km_lapse=(up_delta_1km_lapse/cross_up_1km_lapse)*100
    up_pc_2km_lapse=(up_delta_2km_lapse/cross_up_2km_lapse)*100
    up_pc_3km_lapse=(up_delta_3km_lapse/cross_up_3km_lapse)*100
    up_pc_6km_lapse=(up_delta_6km_lapse/cross_up_6km_lapse)*100
    up_pc_mid_rh=(up_delta_mid_rh/cross_up_mid_rh)*100
    up_pc_sfc_theta=(up_delta_sfc_theta/cross_up_sfc_theta)*100
    up_pc_ml_theta=(up_delta_ml_theta/cross_up_ml_theta)*100
    up_pc_dcape=(up_delta_dcape/cross_up_dcape)*100
    
    #Peak
    peak_pc_lcl=(peak_delta_lcl/cross_peak_lcl)*100
    peak_pc_lfc=(peak_delta_lfc/cross_peak_lfc)*100
    peak_pc_el=(peak_delta_el/cross_peak_el)*100
    peak_pc_h0=(peak_delta_h0/cross_peak_h0)*100
    peak_pc_h0=(peak_delta_h0/cross_peak_h0)*100
    peak_pc_h_neg20=(peak_delta_h_neg20/cross_peak_h_neg20)*100
    peak_pc_wbzh=(peak_delta_wbzh/cross_peak_wbzh)*100
    peak_pc_wbzh=(peak_delta_wbzh/cross_peak_wbzh)*100
    peak_pc_1km_srh=(peak_delta_1km_srh/cross_peak_1km_srh)*100
    peak_pc_1km_shear=(peak_delta_1km_shear/cross_peak_1km_shear)*100
    peak_pc_3km_srh=(peak_delta_3km_srh/cross_peak_3km_srh)*100
    peak_pc_3km_shear=(peak_delta_3km_shear/cross_peak_3km_shear)*100
    peak_pc_6km_shear=(peak_delta_6km_shear/cross_peak_6km_shear)*100
    peak_pc_sbcape=(peak_delta_sbcape/cross_peak_sbcape)*100
    peak_pc_sbcin=(peak_delta_sbcin/cross_peak_sbcin)*100
    peak_pc_mlcape=(peak_delta_mlcape/cross_peak_mlcape)*100
    peak_pc_mlcin=(peak_delta_mlcin/cross_peak_mlcin)*100
    peak_pc_mucape=(peak_delta_mucape/cross_peak_mucape)*100
    peak_pc_mucin=(peak_delta_mucin/cross_peak_mucin)*100
    peak_pc_stp=(peak_delta_stp/cross_peak_stp)*100
    peak_pc_scp=(peak_delta_scp/cross_peak_scp)*100
    peak_pc_sherb=(peak_delta_sherb/cross_peak_sherb)*100
    peak_pc_1km_lapse=(peak_delta_1km_lapse/cross_peak_1km_lapse)*100
    peak_pc_2km_lapse=(peak_delta_2km_lapse/cross_peak_2km_lapse)*100
    peak_pc_3km_lapse=(peak_delta_3km_lapse/cross_peak_3km_lapse)*100
    peak_pc_6km_lapse=(peak_delta_6km_lapse/cross_peak_6km_lapse)*100
    peak_pc_mid_rh=(peak_delta_mid_rh/cross_peak_mid_rh)*100
    peak_pc_sfc_theta=(peak_delta_sfc_theta/cross_peak_sfc_theta)*100
    peak_pc_ml_theta=(peak_delta_ml_theta/cross_peak_ml_theta)*100
    peak_pc_dcape=(peak_delta_dcape/cross_peak_dcape)*100

    
    #Downstream
    down_pc_lcl=(down_delta_lcl/cross_down_lcl)*100
    down_pc_lfc=(down_delta_lfc/cross_down_lfc)*100
    down_pc_el=(down_delta_el/cross_down_el)*100
