    non_delta_up_down_h0_mean=np.mean(non_delta_up_down_h0[np.logical_not(np.isnan(non_delta_up_down_h0))])
    non_delta_up_down_neg20_mean=np.mean(non_delta_up_down_neg20[np.logical_not(np.isnan(non_delta_up_down_neg20))])
    non_delta_up_down_wbzh_mean=np.mean(non_delta_up_down_wbzh[np.logical_not(np.isnan(non_delta_up_down_wbzh))])
    non_delta_up_down_1km_srh_mean=np.mean(non_delta_up_down_1km_srh[np.logical_not(np.isnan(non_delta_up_down_1km_srh))])
    non_delta_up_down_3km_srh_mean=np.mean(non_delta_up_down_3km_srh[np.logical_not(np.isnan(non_delta_up_down_3km_srh))])
    non_delta_up_down_1km_shear_mean=np.mean(non_delta_up_down_1km_shear[np.logical_not(np.isnan(non_delta_up_down_1km_shear))])
    non_delta_up_down_3km_shear_mean=np.mean(non_delta_up_down_3km_shear[np.logical_not(np.isnan(non_delta_up_down_3km_shear))])
    non_delta_up_down_6km_shear_mean=np.mean(non_delta_up_down_6km_shear[np.logical_not(np.isnan(non_delta_up_down_6km_shear))])
    non_delta_up_down_sbcape_mean=np.mean(non_delta_up_down_sbcape[np.logical_not(np.isnan(non_delta_up_down_sbcape))])
    non_delta_up_down_mlcape_mean=np.mean(non_delta_up_down_mlcape[np.logical_not(np.isnan(non_delta_up_down_mlcape))])
