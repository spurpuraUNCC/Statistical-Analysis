        cross_delta_up_down_mucin)],non_delta_up_down_mucin[np.isfinite(non_delta_up_down_mucin)])
    cross_up_down_stp_t,cross_up_down_stp_pvalue_t=stats.ttest_ind(cross_delta_up_down_stp[np.isfinite(
        cross_delta_up_down_stp)],non_delta_up_down_stp[np.isfinite(non_delta_up_down_stp)])
    cross_up_down_scp_t,cross_up_down_scp_pvalue_t=stats.ttest_ind(cross_delta_up_down_scp[np.isfinite(
        cross_delta_up_down_scp)],non_delta_up_down_scp[np.isfinite(non_delta_up_down_scp)])
    cross_up_down_sherb_t,cross_up_down_sherb_pvalue_t=stats.ttest_ind(cross_delta_up_down_sherb[np.isfinite(
        cross_delta_up_down_sherb)],non_delta_up_down_sherb[np.isfinite(non_delta_up_down_sherb)])
    cross_up_down_1km_lapse_t,cross_up_down_1km_lapse_pvalue_t=stats.ttest_ind(cross_delta_up_down_1km_lapse[np.isfinite(
        cross_delta_up_down_1km_lapse)],non_delta_up_down_1km_lapse[np.isfinite(non_delta_up_down_1km_lapse)])
    cross_up_down_2km_lapse_t,cross_up_down_2km_lapse_pvalue_t=stats.ttest_ind(cross_delta_up_down_2km_lapse[np.isfinite(
        cross_delta_up_down_2km_lapse)],non_delta_up_down_2km_lapse[np.isfinite(non_delta_up_down_2km_lapse)])
    cross_up_down_3km_lapse_t,cross_up_down_3km_lapse_pvalue_t=stats.ttest_ind(cross_delta_up_down_3km_lapse[np.isfinite(
        cross_delta_up_down_3km_lapse)],non_delta_up_down_3km_lapse[np.isfinite(non_delta_up_down_3km_lapse)])
    cross_up_down_6km_lapse_t,cross_up_down_6km_lapse_pvalue_t=stats.ttest_ind(cross_delta_up_down_6km_lapse[np.isfinite(
        cross_delta_up_down_6km_lapse)],non_delta_up_down_6km_lapse[np.isfinite(non_delta_up_down_6km_lapse)])
    cross_up_down_mid_rh_t,cross_up_down_mid_rh_pvalue_t=stats.ttest_ind(cross_delta_up_down_mid_rh[np.isfinite(
        cross_delta_up_down_mid_rh)],non_delta_up_down_mid_rh[np.isfinite(non_delta_up_down_mid_rh)])
    cross_up_down_sfc_theta_t,cross_up_down_sfc_theta_pvalue_t=stats.ttest_ind(cross_delta_up_down_sfc_theta[np.isfinite(
        cross_delta_up_down_sfc_theta)],non_delta_up_down_sfc_theta[np.isfinite(non_delta_up_down_sfc_theta)])
    cross_up_down_ml_theta_t,cross_up_down_ml_theta_pvalue_t=stats.ttest_ind(cross_delta_up_down_ml_theta[np.isfinite(
        cross_delta_up_down_ml_theta)],non_delta_up_down_ml_theta[np.isfinite(non_delta_up_down_ml_theta)])
    cross_up_down_dcape_t,cross_up_down_dcape_pvalue_t=stats.ttest_ind(cross_delta_up_down_dcape[np.isfinite(
        cross_delta_up_down_dcape)],non_delta_up_down_dcape[np.isfinite(non_delta_up_down_dcape)])    

    #Upstream vs peak 
    cross_up_peak_lcl_t,cross_up_peak_lcl_pvalue_t=stats.ttest_ind(cross_delta_up_peak_lcl[np.isfinite(
        cross_delta_up_peak_lcl)],non_delta_up_peak_lcl[np.isfinite(non_delta_up_peak_lcl)])
