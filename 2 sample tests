    non_pc_peak_down_6km_shear_mean=np.mean(non_pc_peak_down_6km_shear[np.isfinite(non_pc_peak_down_6km_shear)])
    non_pc_peak_down_sbcape_mean=np.mean(non_pc_peak_down_sbcape[np.isfinite(non_pc_peak_down_sbcape)])
    non_pc_peak_down_mlcape_mean=np.mean(non_pc_peak_down_mlcape[np.isfinite(non_pc_peak_down_mlcape)])
    non_pc_peak_down_mucape_mean=np.mean(non_pc_peak_down_mucape[np.isfinite(non_pc_peak_down_mucape)])
    non_pc_peak_down_sbcin_mean=np.mean(non_pc_peak_down_sbcin[np.isfinite(non_pc_peak_down_sbcin)])
    non_pc_peak_down_mlcin_mean=np.mean(non_pc_peak_down_mlcin[np.isfinite(non_pc_peak_down_mlcin)])
    non_pc_peak_down_mucin_mean=np.mean(non_pc_peak_down_mucin[np.isfinite(non_pc_peak_down_mucin)])
    non_pc_peak_down_stp_mean=np.mean(non_pc_peak_down_stp[np.isfinite(non_pc_peak_down_stp)])
    non_pc_peak_down_scp_mean=np.mean(non_pc_peak_down_scp[np.isfinite(non_pc_peak_down_scp)])
    non_pc_peak_down_sherb_mean=np.mean(non_pc_peak_down_sherb[np.isfinite(non_pc_peak_down_sherb)])
    non_pc_peak_down_1km_lapse_mean=np.mean(non_pc_peak_down_1km_lapse[np.isfinite(non_pc_peak_down_1km_lapse)])
    non_pc_peak_down_2km_lapse_mean=np.mean(non_pc_peak_down_2km_lapse[np.isfinite(non_pc_peak_down_2km_lapse)])
    non_pc_peak_down_3km_lapse_mean=np.mean(non_pc_peak_down_3km_lapse[np.isfinite(non_pc_peak_down_3km_lapse)])
    non_pc_peak_down_6km_lapse_mean=np.mean(non_pc_peak_down_6km_lapse[np.isfinite(non_pc_peak_down_6km_lapse)])
    non_pc_peak_down_mid_rh_mean=np.mean(non_pc_peak_down_mid_rh[np.isfinite(non_pc_peak_down_mid_rh)])
    non_pc_peak_down_sfc_theta_mean=np.mean(non_pc_peak_down_sfc_theta[np.isfinite(non_pc_peak_down_sfc_theta)])
    non_pc_peak_down_ml_theta_mean=np.mean(non_pc_peak_down_ml_theta[np.isfinite(non_pc_peak_down_ml_theta)])
    non_pc_peak_down_dcape_mean=np.mean(non_pc_peak_down_dcape[np.isfinite(non_pc_peak_down_dcape)])
                
    
#Calculating the KS stats and p-values 
    #Upstream
    up_lcl_ks,up_lcl_pvalue=(stats.ks_2samp(cross_up_lcl[np.isfinite(cross_up_lcl)], 
                                            non_up_lcl[np.isfinite(non_up_lcl)]))
    up_lfc_ks,up_lfc_pvalue=(stats.ks_2samp(cross_up_lfc[np.isfinite(cross_up_lfc)], 
                                            non_up_lfc[np.isfinite(non_up_lfc)]))
    up_el_ks,up_el_pvalue=(stats.ks_2samp(cross_up_el[np.isfinite(cross_up_el)], 
                                          non_up_el[np.isfinite(non_up_el)]))
    up_h0_ks,up_h0_pvalue=(stats.ks_2samp(cross_up_h0[np.isfinite(cross_up_h0)],
                                          non_up_h0[np.isfinite(non_up_h0)]))
    up_h_neg20_ks,up_h_neg20_pvalue=(stats.ks_2samp(cross_up_h_neg20[np.isfinite(cross_up_h_neg20)], 
                                                    non_up_h_neg20[np.isfinite(non_up_h_neg20)]))
    up_wbzh_ks,up_wbzh_pvalue=(stats.ks_2samp(cross_up_wbzh[np.isfinite(cross_up_wbzh)], 
                                              non_up_wbzh[np.isfinite(non_up_wbzh)]))
    up_1km_srh_ks,up_1km_srh_pvalue=(stats.ks_2samp(cross_up_1km_srh[np.isfinite(cross_up_1km_srh)],
                                                    non_up_1km_srh[np.isfinite(non_up_1km_srh)]))
    up_3km_srh_ks,up_3km_srh_pvalue=(stats.ks_2samp(cross_up_3km_srh[np.isfinite(cross_up_3km_srh)],
                                                    non_up_3km_srh[np.isfinite(non_up_3km_srh)]))
    up_1km_shear_ks,up_1km_shear_pvalue=(stats.ks_2samp(cross_up_1km_shear[np.isfinite(cross_up_1km_shear)],
                                                    non_up_1km_shear[np.isfinite(non_up_1km_shear)]))
    up_3km_shear_ks,up_3km_shear_pvalue=(stats.ks_2samp(cross_up_3km_shear[np.isfinite(cross_up_3km_shear)],
                                                    non_up_3km_shear[np.isfinite(non_up_3km_shear)]))
    up_6km_shear_ks,up_6km_shear_pvalue=(stats.ks_2samp(cross_up_6km_shear[np.isfinite(cross_up_6km_shear)],
                                                    non_up_6km_shear[np.isfinite(non_up_6km_shear)]))
    up_sbcape_ks,up_sbcape_pvalue=(stats.ks_2samp(cross_up_sbcape[np.isfinite(cross_up_sbcape)],
                                                    non_up_sbcape[np.isfinite(non_up_sbcape)]))
    up_mlcape_ks,up_mlcape_pvalue=(stats.ks_2samp(cross_up_mlcape[np.isfinite(cross_up_mlcape)],
                                                    non_up_mlcape[np.isfinite(non_up_mlcape)]))
    up_mucape_ks,up_mucape_pvalue=(stats.ks_2samp(cross_up_mucape[np.isfinite(cross_up_mucape)],
                                                    non_up_mucape[np.isfinite(non_up_mucape)]))
    up_sbcin_ks,up_sbcin_pvalue=(stats.ks_2samp(cross_up_sbcin[np.isfinite(cross_up_sbcin)],
                                                    non_up_sbcin[np.isfinite(non_up_sbcin)]))
    up_mlcin_ks,up_mlcin_pvalue=(stats.ks_2samp(cross_up_mlcin[np.isfinite(cross_up_mlcin)],
                                                    non_up_mlcin[np.isfinite(non_up_mlcin)]))
    up_mucin_ks,up_mucin_pvalue=(stats.ks_2samp(cross_up_mucin[np.isfinite(cross_up_mucin)],
                                                    non_up_mucin[np.isfinite(non_up_mucin)]))
    up_stp_ks,up_stp_pvalue=(stats.ks_2samp(cross_up_stp[np.isfinite(cross_up_stp)],
                                                    non_up_stp[np.isfinite(non_up_stp)]))
    up_scp_ks,up_scp_pvalue=(stats.ks_2samp(cross_up_scp[np.isfinite(cross_up_scp)],
                                                    non_up_scp[np.isfinite(non_up_scp)]))
    up_sherb_ks,up_sherb_pvalue=(stats.ks_2samp(cross_up_sherb[np.isfinite(cross_up_sherb)],
                                                    non_up_sherb[np.isfinite(non_up_sherb)]))
    up_1km_lapse_ks,up_1km_lapse_pvalue=(stats.ks_2samp(cross_up_1km_lapse[np.isfinite(cross_up_1km_lapse)],
                                                    non_up_1km_lapse[np.isfinite(non_up_1km_lapse)]))
    up_2km_lapse_ks,up_2km_lapse_pvalue=(stats.ks_2samp(cross_up_2km_lapse[np.isfinite(cross_up_2km_lapse)],
                                                    non_up_2km_lapse[np.isfinite(non_up_2km_lapse)]))
    up_3km_lapse_ks,up_3km_lapse_pvalue=(stats.ks_2samp(cross_up_3km_lapse[np.isfinite(cross_up_3km_lapse)],
                                                    non_up_3km_lapse[np.isfinite(non_up_3km_lapse)]))
    up_6km_lapse_ks,up_6km_lapse_pvalue=(stats.ks_2samp(cross_up_6km_lapse[np.isfinite(cross_up_6km_lapse)],
                                                    non_up_6km_lapse[np.isfinite(non_up_6km_lapse)]))
    up_mid_rh_ks,up_mid_rh_pvalue=(stats.ks_2samp(cross_up_mid_rh[np.isfinite(cross_up_mid_rh)],
                                                    non_up_mid_rh[np.isfinite(non_up_mid_rh)]))
    up_sfc_theta_ks,up_sfc_theta_pvalue=(stats.ks_2samp(cross_up_sfc_theta[np.isfinite(cross_up_sfc_theta)],
                                                    non_up_sfc_theta[np.isfinite(non_up_sfc_theta)]))
    up_ml_theta_ks,up_ml_theta_pvalue=(stats.ks_2samp(cross_up_ml_theta[np.isfinite(cross_up_ml_theta)],
                                                    non_up_ml_theta[np.isfinite(non_up_ml_theta)]))
    up_dcape_ks,up_dcape_pvalue=(stats.ks_2samp(cross_up_dcape[np.isfinite(cross_up_dcape)],
                                                    non_up_dcape[np.isfinite(non_up_dcape)]))
    
    #Peak
    peak_lcl_ks,peak_lcl_pvalue=(stats.ks_2samp(cross_peak_lcl[np.isfinite(cross_peak_lcl)], 
                                            non_peak_lcl[np.isfinite(non_peak_lcl)]))
    peak_lfc_ks,peak_lfc_pvalue=(stats.ks_2samp(cross_peak_lfc[np.isfinite(cross_peak_lfc)], 
                                            non_peak_lfc[np.isfinite(non_peak_lfc)]))
    peak_el_ks,peak_el_pvalue=(stats.ks_2samp(cross_peak_el[np.isfinite(cross_peak_el)], 
                                          non_peak_el[np.isfinite(non_peak_el)]))
    peak_h0_ks,peak_h0_pvalue=(stats.ks_2samp(cross_peak_h0[np.isfinite(cross_peak_h0)],
                                          non_peak_h0[np.isfinite(non_peak_h0)]))
    peak_h_neg20_ks,peak_h_neg20_pvalue=(stats.ks_2samp(cross_peak_h_neg20[np.isfinite(cross_peak_h_neg20)], 
                                                    non_peak_h_neg20[np.isfinite(non_peak_h_neg20)]))
    peak_wbzh_ks,peak_wbzh_pvalue=(stats.ks_2samp(cross_peak_wbzh[np.isfinite(cross_peak_wbzh)], 
                                              non_peak_wbzh[np.isfinite(non_peak_wbzh)]))
    peak_1km_srh_ks,peak_1km_srh_pvalue=(stats.ks_2samp(cross_peak_1km_srh[np.isfinite(cross_peak_1km_srh)],
                                                    non_peak_1km_srh[np.isfinite(non_peak_1km_srh)]))
    peak_3km_srh_ks,peak_3km_srh_pvalue=(stats.ks_2samp(cross_peak_3km_srh[np.isfinite(cross_peak_3km_srh)],
                                                    non_peak_3km_srh[np.isfinite(non_peak_3km_srh)]))
    peak_1km_shear_ks,peak_1km_shear_pvalue=(stats.ks_2samp(cross_peak_1km_shear[np.isfinite(cross_peak_1km_shear)],
                                                    non_peak_1km_shear[np.isfinite(non_peak_1km_shear)]))
    peak_3km_shear_ks,peak_3km_shear_pvalue=(stats.ks_2samp(cross_peak_3km_shear[np.isfinite(cross_peak_3km_shear)],
                                                    non_peak_3km_shear[np.isfinite(non_peak_3km_shear)]))
    peak_6km_shear_ks,peak_6km_shear_pvalue=(stats.ks_2samp(cross_peak_6km_shear[np.isfinite(cross_peak_6km_shear)],
                                                    non_peak_6km_shear[np.isfinite(non_peak_6km_shear)]))
    peak_sbcape_ks,peak_sbcape_pvalue=(stats.ks_2samp(cross_peak_sbcape[np.isfinite(cross_peak_sbcape)],
                                                    non_peak_sbcape[np.isfinite(non_peak_sbcape)]))
    peak_mlcape_ks,peak_mlcape_pvalue=(stats.ks_2samp(cross_peak_mlcape[np.isfinite(cross_peak_mlcape)],
                                                    non_peak_mlcape[np.isfinite(non_peak_mlcape)]))
    peak_mucape_ks,peak_mucape_pvalue=(stats.ks_2samp(cross_peak_mucape[np.isfinite(cross_peak_mucape)],
                                                    non_peak_mucape[np.isfinite(non_peak_mucape)]))
    peak_sbcin_ks,peak_sbcin_pvalue=(stats.ks_2samp(cross_peak_sbcin[np.isfinite(cross_peak_sbcin)],
                                                    non_peak_sbcin[np.isfinite(non_peak_sbcin)]))
    peak_mlcin_ks,peak_mlcin_pvalue=(stats.ks_2samp(cross_peak_mlcin[np.isfinite(cross_peak_mlcin)],
                                                    non_peak_mlcin[np.isfinite(non_peak_mlcin)]))
    peak_mucin_ks,peak_mucin_pvalue=(stats.ks_2samp(cross_peak_mucin[np.isfinite(cross_peak_mucin)],
                                                    non_peak_mucin[np.isfinite(non_peak_mucin)]))
    peak_stp_ks,peak_stp_pvalue=(stats.ks_2samp(cross_peak_stp[np.isfinite(cross_peak_stp)],
                                                    non_peak_stp[np.isfinite(non_peak_stp)]))
    peak_scp_ks,peak_scp_pvalue=(stats.ks_2samp(cross_peak_scp[np.isfinite(cross_peak_scp)],
                                                    non_peak_scp[np.isfinite(non_peak_scp)]))
    peak_sherb_ks,peak_sherb_pvalue=(stats.ks_2samp(cross_peak_sherb[np.isfinite(cross_peak_sherb)],
                                                    non_peak_sherb[np.isfinite(non_peak_sherb)]))
    peak_1km_lapse_ks,peak_1km_lapse_pvalue=(stats.ks_2samp(cross_peak_1km_lapse[np.isfinite(cross_peak_1km_lapse)],
                                                    non_peak_1km_lapse[np.isfinite(non_peak_1km_lapse)]))
    peak_2km_lapse_ks,peak_2km_lapse_pvalue=(stats.ks_2samp(cross_peak_2km_lapse[np.isfinite(cross_peak_2km_lapse)],
                                                    non_peak_2km_lapse[np.isfinite(non_peak_2km_lapse)]))
    peak_3km_lapse_ks,peak_3km_lapse_pvalue=(stats.ks_2samp(cross_peak_3km_lapse[np.isfinite(cross_peak_3km_lapse)],
                                                    non_peak_3km_lapse[np.isfinite(non_peak_3km_lapse)]))
    peak_6km_lapse_ks,peak_6km_lapse_pvalue=(stats.ks_2samp(cross_peak_6km_lapse[np.isfinite(cross_peak_6km_lapse)],
                                                    non_peak_6km_lapse[np.isfinite(non_peak_6km_lapse)]))
    peak_mid_rh_ks,peak_mid_rh_pvalue=(stats.ks_2samp(cross_peak_mid_rh[np.isfinite(cross_peak_mid_rh)],
                                                    non_peak_mid_rh[np.isfinite(non_peak_mid_rh)]))
    peak_sfc_theta_ks,peak_sfc_theta_pvalue=(stats.ks_2samp(cross_peak_sfc_theta[np.isfinite(cross_peak_sfc_theta)],
                                                    non_peak_sfc_theta[np.isfinite(non_peak_sfc_theta)]))
    peak_ml_theta_ks,peak_ml_theta_pvalue=(stats.ks_2samp(cross_peak_ml_theta[np.isfinite(cross_peak_ml_theta)],
                                                    non_peak_ml_theta[np.isfinite(non_peak_ml_theta)]))
    peak_dcape_ks,peak_dcape_pvalue=(stats.ks_2samp(cross_peak_dcape[np.isfinite(cross_peak_dcape)],
                                                    non_peak_dcape[np.isfinite(non_peak_dcape)]))

    #Downstream
    down_lcl_ks,down_lcl_pvalue=(stats.ks_2samp(cross_down_lcl[np.isfinite(cross_down_lcl)], 
                                            non_down_lcl[np.isfinite(non_down_lcl)]))
    down_lfc_ks,down_lfc_pvalue=(stats.ks_2samp(cross_down_lfc[np.isfinite(cross_down_lfc)], 
                                            non_down_lfc[np.isfinite(non_down_lfc)]))
    down_el_ks,down_el_pvalue=(stats.ks_2samp(cross_down_el[np.isfinite(cross_down_el)], 
                                          non_down_el[np.isfinite(non_down_el)]))
    down_h0_ks,down_h0_pvalue=(stats.ks_2samp(cross_down_h0[np.isfinite(cross_down_h0)],
                                          non_down_h0[np.isfinite(non_down_h0)]))
    down_h_neg20_ks,down_h_neg20_pvalue=(stats.ks_2samp(cross_down_h_neg20[np.isfinite(cross_down_h_neg20)], 
                                                    non_down_h_neg20[np.isfinite(non_down_h_neg20)]))
    down_wbzh_ks,down_wbzh_pvalue=(stats.ks_2samp(cross_down_wbzh[np.isfinite(cross_down_wbzh)], 
                                              non_down_wbzh[np.isfinite(non_down_wbzh)]))
    down_1km_srh_ks,down_1km_srh_pvalue=(stats.ks_2samp(cross_down_1km_srh[np.isfinite(cross_down_1km_srh)],
                                                    non_down_1km_srh[np.isfinite(non_down_1km_srh)]))
    down_3km_srh_ks,down_3km_srh_pvalue=(stats.ks_2samp(cross_down_3km_srh[np.isfinite(cross_down_3km_srh)],
                                                    non_down_3km_srh[np.isfinite(non_down_3km_srh)]))
    down_1km_shear_ks,down_1km_shear_pvalue=(stats.ks_2samp(cross_down_1km_shear[np.isfinite(cross_down_1km_shear)],
                                                    non_down_1km_shear[np.isfinite(non_down_1km_shear)]))
    down_3km_shear_ks,down_3km_shear_pvalue=(stats.ks_2samp(cross_down_3km_shear[np.isfinite(cross_down_3km_shear)],
                                                    non_down_3km_shear[np.isfinite(non_down_3km_shear)]))
    down_6km_shear_ks,down_6km_shear_pvalue=(stats.ks_2samp(cross_down_6km_shear[np.isfinite(cross_down_6km_shear)],
                                                    non_down_6km_shear[np.isfinite(non_down_6km_shear)]))
    down_sbcape_ks,down_sbcape_pvalue=(stats.ks_2samp(cross_down_sbcape[np.isfinite(cross_down_sbcape)],
                                                    non_down_sbcape[np.isfinite(non_down_sbcape)]))
    down_mlcape_ks,down_mlcape_pvalue=(stats.ks_2samp(cross_down_mlcape[np.isfinite(cross_down_mlcape)],
                                                    non_down_mlcape[np.isfinite(non_down_mlcape)]))
    down_mucape_ks,down_mucape_pvalue=(stats.ks_2samp(cross_down_mucape[np.isfinite(cross_down_mucape)],
                                                    non_down_mucape[np.isfinite(non_down_mucape)]))
    down_sbcin_ks,down_sbcin_pvalue=(stats.ks_2samp(cross_down_sbcin[np.isfinite(cross_down_sbcin)],
                                                    non_down_sbcin[np.isfinite(non_down_sbcin)]))
    down_mlcin_ks,down_mlcin_pvalue=(stats.ks_2samp(cross_down_mlcin[np.isfinite(cross_down_mlcin)],
                                                    non_down_mlcin[np.isfinite(non_down_mlcin)]))
    down_mucin_ks,down_mucin_pvalue=(stats.ks_2samp(cross_down_mucin[np.isfinite(cross_down_mucin)],
                                                    non_down_mucin[np.isfinite(non_down_mucin)]))
    down_stp_ks,down_stp_pvalue=(stats.ks_2samp(cross_down_stp[np.isfinite(cross_down_stp)],
                                                    non_down_stp[np.isfinite(non_down_stp)]))
    down_scp_ks,down_scp_pvalue=(stats.ks_2samp(cross_down_scp[np.isfinite(cross_down_scp)],
                                                    non_down_scp[np.isfinite(non_down_scp)]))
    down_sherb_ks,down_sherb_pvalue=(stats.ks_2samp(cross_down_sherb[np.isfinite(cross_down_sherb)],
                                                    non_down_sherb[np.isfinite(non_down_sherb)]))
    down_1km_lapse_ks,down_1km_lapse_pvalue=(stats.ks_2samp(cross_down_1km_lapse[np.isfinite(cross_down_1km_lapse)],
                                                    non_down_1km_lapse[np.isfinite(non_down_1km_lapse)]))
    down_2km_lapse_ks,down_2km_lapse_pvalue=(stats.ks_2samp(cross_down_2km_lapse[np.isfinite(cross_down_2km_lapse)],
                                                    non_down_2km_lapse[np.isfinite(non_down_2km_lapse)]))
    down_3km_lapse_ks,down_3km_lapse_pvalue=(stats.ks_2samp(cross_down_3km_lapse[np.isfinite(cross_down_3km_lapse)],
                                                    non_down_3km_lapse[np.isfinite(non_down_3km_lapse)]))
    down_6km_lapse_ks,down_6km_lapse_pvalue=(stats.ks_2samp(cross_down_6km_lapse[np.isfinite(cross_down_6km_lapse)],
                                                    non_down_6km_lapse[np.isfinite(non_down_6km_lapse)]))
    down_mid_rh_ks,down_mid_rh_pvalue=(stats.ks_2samp(cross_down_mid_rh[np.isfinite(cross_down_mid_rh)],
                                                    non_down_mid_rh[np.isfinite(non_down_mid_rh)]))
    down_sfc_theta_ks,down_sfc_theta_pvalue=(stats.ks_2samp(cross_down_sfc_theta[np.isfinite(cross_down_sfc_theta)],
                                                    non_down_sfc_theta[np.isfinite(non_down_sfc_theta)]))
    down_ml_theta_ks,down_ml_theta_pvalue=(stats.ks_2samp(cross_down_ml_theta[np.isfinite(cross_down_ml_theta)],
                                                    non_down_ml_theta[np.isfinite(non_down_ml_theta)]))
    down_dcape_ks,down_dcape_pvalue=(stats.ks_2samp(cross_down_dcape[np.isfinite(cross_down_dcape)],
                                                    non_down_dcape[np.isfinite(non_down_dcape)]))


    ##Upstream vs. Downstream difference
    #Crossers
    cross_up_down_lcl_ks,cross_up_down_lcl_pvalue=stats.ks_2samp(cross_delta_up_down_lcl[np.isfinite(
        cross_delta_up_down_lcl)],non_delta_up_down_lcl[np.isfinite(non_delta_up_down_lcl)])
    cross_up_down_lfc_ks,cross_up_down_lfc_pvalue=stats.ks_2samp(cross_delta_up_down_lfc[np.isfinite(
        cross_delta_up_down_lfc)],non_delta_up_down_lfc[np.isfinite(non_delta_up_down_lfc)])
    cross_up_down_el_ks,cross_up_down_el_pvalue=stats.ks_2samp(cross_delta_up_down_el[np.isfinite(
        cross_delta_up_down_el)],non_delta_up_down_el[np.isfinite(non_delta_up_down_el)])
    cross_up_down_h0_ks,cross_up_down_h0_pvalue=stats.ks_2samp(cross_delta_up_down_h0[np.isfinite(
        cross_delta_up_down_h0)],non_delta_up_down_h0[np.isfinite(non_delta_up_down_h0)])
    cross_up_down_neg20_ks,cross_up_down_neg20_pvalue=stats.ks_2samp(cross_delta_up_down_neg20[np.isfinite(
        cross_delta_up_down_neg20)],non_delta_up_down_neg20[np.isfinite(non_delta_up_down_neg20)])
    cross_up_down_wbzh_ks,cross_up_down_wbzh_pvalue=stats.ks_2samp(cross_delta_up_down_wbzh[np.isfinite(
        cross_delta_up_down_wbzh)],non_delta_up_down_wbzh[np.isfinite(non_delta_up_down_wbzh)])
    cross_up_down_1km_srh_ks,cross_up_down_1km_srh_pvalue=stats.ks_2samp(cross_delta_up_down_1km_srh[np.isfinite(
        cross_delta_up_down_1km_srh)],non_delta_up_down_1km_srh[np.isfinite(non_delta_up_down_1km_srh)])
    cross_up_down_3km_srh_ks,cross_up_down_3km_srh_pvalue=stats.ks_2samp(cross_delta_up_down_3km_srh[np.isfinite(
        cross_delta_up_down_3km_srh)],non_delta_up_down_3km_srh[np.isfinite(non_delta_up_down_3km_srh)])
    cross_up_down_1km_shear_ks,cross_up_down_1km_shear_pvalue=stats.ks_2samp(cross_delta_up_down_1km_shear[np.isfinite(
        cross_delta_up_down_1km_shear)],non_delta_up_down_1km_shear[np.isfinite(non_delta_up_down_1km_shear)])
    cross_up_down_3km_shear_ks,cross_up_down_3km_shear_pvalue=stats.ks_2samp(cross_delta_up_down_3km_shear[np.isfinite(
        cross_delta_up_down_3km_shear)],non_delta_up_down_3km_shear[np.isfinite(non_delta_up_down_3km_shear)])
    cross_up_down_6km_shear_ks,cross_up_down_6km_shear_pvalue=stats.ks_2samp(cross_delta_up_down_6km_shear[np.isfinite(
        cross_delta_up_down_6km_shear)],non_delta_up_down_6km_shear[np.isfinite(non_delta_up_down_6km_shear)])
    cross_up_down_sbcape_ks,cross_up_down_sbcape_pvalue=stats.ks_2samp(cross_delta_up_down_sbcape[np.isfinite(
        cross_delta_up_down_sbcape)],non_delta_up_down_sbcape[np.isfinite(non_delta_up_down_sbcape)])
    cross_up_down_mlcape_ks,cross_up_down_mlcape_pvalue=stats.ks_2samp(cross_delta_up_down_mlcape[np.isfinite(
        cross_delta_up_down_mlcape)],non_delta_up_down_mlcape[np.isfinite(non_delta_up_down_mlcape)])
    cross_up_down_mucape_ks,cross_up_down_mucape_pvalue=stats.ks_2samp(cross_delta_up_down_mucape,non_delta_up_down_mucape)
    cross_up_down_sbcin_ks,cross_up_down_sbcin_pvalue=stats.ks_2samp(cross_delta_up_down_sbcin[np.isfinite(
        cross_delta_up_down_sbcin)],non_delta_up_down_sbcin[np.isfinite(non_delta_up_down_sbcin)])
    cross_up_down_mlcin_ks,cross_up_down_mlcin_pvalue=stats.ks_2samp(cross_delta_up_down_mlcin[np.isfinite(
        cross_delta_up_down_mlcin)],non_delta_up_down_mlcin[np.isfinite(non_delta_up_down_mlcin)])
    cross_up_down_mucin_ks,cross_up_down_mucin_pvalue=stats.ks_2samp(cross_delta_up_down_mucin[np.isfinite(
        cross_delta_up_down_mucin)],non_delta_up_down_mucin[np.isfinite(non_delta_up_down_mucin)])
    cross_up_down_stp_ks,cross_up_down_stp_pvalue=stats.ks_2samp(cross_delta_up_down_stp[np.isfinite(
        cross_delta_up_down_stp)],non_delta_up_down_stp[np.isfinite(non_delta_up_down_stp)])
    cross_up_down_scp_ks,cross_up_down_scp_pvalue=stats.ks_2samp(cross_delta_up_down_scp[np.isfinite(
        cross_delta_up_down_scp)],non_delta_up_down_scp[np.isfinite(non_delta_up_down_scp)])
    cross_up_down_sherb_ks,cross_up_down_sherb_pvalue=stats.ks_2samp(cross_delta_up_down_sherb[np.isfinite(
        cross_delta_up_down_sherb)],non_delta_up_down_sherb[np.isfinite(non_delta_up_down_sherb)])
    cross_up_down_1km_lapse_ks,cross_up_down_1km_lapse_pvalue=stats.ks_2samp(cross_delta_up_down_1km_lapse[np.isfinite(
        cross_delta_up_down_1km_lapse)],non_delta_up_down_1km_lapse[np.isfinite(non_delta_up_down_1km_lapse)])
    cross_up_down_2km_lapse_ks,cross_up_down_2km_lapse_pvalue=stats.ks_2samp(cross_delta_up_down_2km_lapse[np.isfinite(
        cross_delta_up_down_2km_lapse)],non_delta_up_down_2km_lapse[np.isfinite(non_delta_up_down_2km_lapse)])
    cross_up_down_3km_lapse_ks,cross_up_down_3km_lapse_pvalue=stats.ks_2samp(cross_delta_up_down_3km_lapse[np.isfinite(
        cross_delta_up_down_3km_lapse)],non_delta_up_down_3km_lapse[np.isfinite(non_delta_up_down_3km_lapse)])
    cross_up_down_6km_lapse_ks,cross_up_down_6km_lapse_pvalue=stats.ks_2samp(cross_delta_up_down_6km_lapse[np.isfinite(
        cross_delta_up_down_6km_lapse)],non_delta_up_down_6km_lapse[np.isfinite(non_delta_up_down_6km_lapse)])
    cross_up_down_mid_rh_ks,cross_up_down_mid_rh_pvalue=stats.ks_2samp(cross_delta_up_down_mid_rh[np.isfinite(
        cross_delta_up_down_mid_rh)],non_delta_up_down_mid_rh[np.isfinite(non_delta_up_down_mid_rh)])
    cross_up_down_sfc_theta_ks,cross_up_down_sfc_theta_pvalue=stats.ks_2samp(cross_delta_up_down_sfc_theta[np.isfinite(
        cross_delta_up_down_sfc_theta)],non_delta_up_down_sfc_theta[np.isfinite(non_delta_up_down_sfc_theta)])
    cross_up_down_ml_theta_ks,cross_up_down_ml_theta_pvalue=stats.ks_2samp(cross_delta_up_down_ml_theta[np.isfinite(
        cross_delta_up_down_ml_theta)],non_delta_up_down_ml_theta[np.isfinite(non_delta_up_down_ml_theta)])
    cross_up_down_dcape_ks,cross_up_down_dcape_pvalue=stats.ks_2samp(cross_delta_up_down_dcape[np.isfinite(
        cross_delta_up_down_dcape)],non_delta_up_down_dcape[np.isfinite(non_delta_up_down_dcape)])
    
    ##Upstream vs. Peak difference
    #Crossers
    cross_up_peak_lcl_ks,cross_up_peak_lcl_pvalue=stats.ks_2samp(cross_delta_up_peak_lcl[np.isfinite(
        cross_delta_up_peak_lcl)],non_delta_up_peak_lcl[np.isfinite(non_delta_up_peak_lcl)])
    cross_up_peak_lfc_ks,cross_up_peak_lfc_pvalue=stats.ks_2samp(cross_delta_up_peak_lfc[np.isfinite(
        cross_delta_up_peak_lfc)],non_delta_up_peak_lfc[np.isfinite(non_delta_up_peak_lfc)])
    cross_up_peak_el_ks,cross_up_peak_el_pvalue=stats.ks_2samp(cross_delta_up_peak_el[np.isfinite(
        cross_delta_up_peak_el)],non_delta_up_peak_el[np.isfinite(non_delta_up_peak_el)])
    cross_up_peak_h0_ks,cross_up_peak_h0_pvalue=stats.ks_2samp(cross_delta_up_peak_h0[np.isfinite(
        cross_delta_up_peak_h0)],non_delta_up_peak_h0[np.isfinite(non_delta_up_peak_h0)])
    cross_up_peak_neg20_ks,cross_up_peak_neg20_pvalue=stats.ks_2samp(cross_delta_up_peak_neg20[np.isfinite(
        cross_delta_up_peak_neg20)],non_delta_up_peak_neg20[np.isfinite(non_delta_up_peak_neg20)])
    cross_up_peak_wbzh_ks,cross_up_peak_wbzh_pvalue=stats.ks_2samp(cross_delta_up_peak_wbzh[np.isfinite(
        cross_delta_up_peak_wbzh)],non_delta_up_peak_wbzh[np.isfinite(non_delta_up_peak_wbzh)])
    cross_up_peak_1km_srh_ks,cross_up_peak_1km_srh_pvalue=stats.ks_2samp(cross_delta_up_peak_1km_srh[np.isfinite(
        cross_delta_up_peak_1km_srh)],non_delta_up_peak_1km_srh[np.isfinite(non_delta_up_peak_1km_srh)])
    cross_up_peak_3km_srh_ks,cross_up_peak_3km_srh_pvalue=stats.ks_2samp(cross_delta_up_peak_3km_srh[np.isfinite(
        cross_delta_up_peak_3km_srh)],non_delta_up_peak_3km_srh[np.isfinite(non_delta_up_peak_3km_srh)])
    cross_up_peak_1km_shear_ks,cross_up_peak_1km_shear_pvalue=stats.ks_2samp(cross_delta_up_peak_1km_shear[np.isfinite(
        cross_delta_up_peak_1km_shear)],non_delta_up_peak_1km_shear[np.isfinite(non_delta_up_peak_1km_shear)])
    cross_up_peak_3km_shear_ks,cross_up_peak_3km_shear_pvalue=stats.ks_2samp(cross_delta_up_peak_3km_shear[np.isfinite(
        cross_delta_up_peak_3km_shear)],non_delta_up_peak_3km_shear[np.isfinite(non_delta_up_peak_3km_shear)])
    cross_up_peak_6km_shear_ks,cross_up_peak_6km_shear_pvalue=stats.ks_2samp(cross_delta_up_peak_6km_shear[np.isfinite(
        cross_delta_up_peak_6km_shear)],non_delta_up_peak_6km_shear[np.isfinite(non_delta_up_peak_6km_shear)])
    cross_up_peak_sbcape_ks,cross_up_peak_sbcape_pvalue=stats.ks_2samp(cross_delta_up_peak_sbcape[np.isfinite(
        cross_delta_up_peak_sbcape)],non_delta_up_peak_sbcape[np.isfinite(non_delta_up_peak_sbcape)])
    cross_up_peak_mlcape_ks,cross_up_peak_mlcape_pvalue=stats.ks_2samp(cross_delta_up_peak_mlcape[np.isfinite(
        cross_delta_up_peak_mlcape)],non_delta_up_peak_mlcape[np.isfinite(non_delta_up_peak_mlcape)])
    cross_up_peak_mucape_ks,cross_up_peak_mucape_pvalue=stats.ks_2samp(cross_delta_up_peak_mucape[np.isfinite(
        cross_delta_up_peak_mucape)],non_delta_up_peak_mucape[np.isfinite(non_delta_up_peak_mucape)])
    cross_up_peak_sbcin_ks,cross_up_peak_sbcin_pvalue=stats.ks_2samp(cross_delta_up_peak_sbcin[np.isfinite(
        cross_delta_up_peak_sbcin)],non_delta_up_peak_sbcin[np.isfinite(non_delta_up_peak_sbcin)])
    cross_up_peak_mlcin_ks,cross_up_peak_mlcin_pvalue=stats.ks_2samp(cross_delta_up_peak_mlcin[np.isfinite(
        cross_delta_up_peak_mlcin)],non_delta_up_peak_mlcin[np.isfinite(non_delta_up_peak_mlcin)])
    cross_up_peak_mucin_ks,cross_up_peak_mucin_pvalue=stats.ks_2samp(cross_delta_up_peak_mucin[np.isfinite(
        cross_delta_up_peak_mucin)],non_delta_up_peak_mucin[np.isfinite(non_delta_up_peak_mucin)])
    cross_up_peak_stp_ks,cross_up_peak_stp_pvalue=stats.ks_2samp(cross_delta_up_peak_stp[np.isfinite(
        cross_delta_up_peak_stp)],non_delta_up_peak_stp[np.isfinite(non_delta_up_peak_stp)])
    cross_up_peak_scp_ks,cross_up_peak_scp_pvalue=stats.ks_2samp(cross_delta_up_peak_scp[np.isfinite(
        cross_delta_up_peak_scp)],non_delta_up_peak_scp[np.isfinite(non_delta_up_peak_scp)])
    cross_up_peak_sherb_ks,cross_up_peak_sherb_pvalue=stats.ks_2samp(cross_delta_up_peak_sherb[np.isfinite(
        cross_delta_up_peak_sherb)],non_delta_up_peak_sherb[np.isfinite(non_delta_up_peak_sherb)])
    cross_up_peak_1km_lapse_ks,cross_up_peak_1km_lapse_pvalue=stats.ks_2samp(cross_delta_up_peak_1km_lapse[np.isfinite(
        cross_delta_up_peak_1km_lapse)],non_delta_up_peak_1km_lapse[np.isfinite(non_delta_up_peak_1km_lapse)])
    cross_up_peak_2km_lapse_ks,cross_up_peak_2km_lapse_pvalue=stats.ks_2samp(cross_delta_up_peak_2km_lapse[np.isfinite(
        cross_delta_up_peak_2km_lapse)],non_delta_up_peak_2km_lapse[np.isfinite(non_delta_up_peak_2km_lapse)])
    cross_up_peak_3km_lapse_ks,cross_up_peak_3km_lapse_pvalue=stats.ks_2samp(cross_delta_up_peak_3km_lapse[np.isfinite(
        cross_delta_up_peak_3km_lapse)],non_delta_up_peak_3km_lapse[np.isfinite(non_delta_up_peak_3km_lapse)])
    cross_up_peak_6km_lapse_ks,cross_up_peak_6km_lapse_pvalue=stats.ks_2samp(cross_delta_up_peak_6km_lapse[np.isfinite(
        cross_delta_up_peak_6km_lapse)],non_delta_up_peak_6km_lapse[np.isfinite(non_delta_up_peak_6km_lapse)])
    cross_up_peak_mid_rh_ks,cross_up_peak_mid_rh_pvalue=stats.ks_2samp(cross_delta_up_peak_mid_rh[np.isfinite(
        cross_delta_up_peak_mid_rh)],non_delta_up_peak_mid_rh[np.isfinite(non_delta_up_peak_mid_rh)])
    cross_up_peak_sfc_theta_ks,cross_up_peak_sfc_theta_pvalue=stats.ks_2samp(cross_delta_up_peak_sfc_theta[np.isfinite(
        cross_delta_up_peak_sfc_theta)],non_delta_up_peak_sfc_theta[np.isfinite(non_delta_up_peak_sfc_theta)])
    cross_up_peak_ml_theta_ks,cross_up_peak_ml_theta_pvalue=stats.ks_2samp(cross_delta_up_peak_ml_theta[np.isfinite(
        cross_delta_up_peak_ml_theta)],non_delta_up_peak_ml_theta[np.isfinite(non_delta_up_peak_ml_theta)])
    cross_up_peak_dcape_ks,cross_up_peak_dcape_pvalue=stats.ks_2samp(cross_delta_up_peak_dcape[np.isfinite(
        cross_delta_up_peak_dcape)],non_delta_up_peak_dcape[np.isfinite(non_delta_up_peak_dcape)])
    
    ##Peak vs. Downstream difference
    #Crossers
    cross_peak_down_lcl_ks,cross_peak_down_lcl_pvalue=stats.ks_2samp(cross_delta_peak_down_lcl[np.isfinite(
        cross_delta_peak_down_lcl)],non_delta_peak_down_lcl[np.isfinite(non_delta_peak_down_lcl)])
    cross_peak_down_lfc_ks,cross_peak_down_lfc_pvalue=stats.ks_2samp(cross_delta_peak_down_lfc[np.isfinite(
        cross_delta_peak_down_lfc)],non_delta_peak_down_lfc[np.isfinite(non_delta_peak_down_lfc)])
    cross_peak_down_el_ks,cross_peak_down_el_pvalue=stats.ks_2samp(cross_delta_peak_down_el[np.isfinite(
        cross_delta_peak_down_el)],non_delta_peak_down_el[np.isfinite(non_delta_peak_down_el)])
    cross_peak_down_h0_ks,cross_peak_down_h0_pvalue=stats.ks_2samp(cross_delta_peak_down_h0[np.isfinite(
        cross_delta_peak_down_h0)],non_delta_peak_down_h0[np.isfinite(non_delta_peak_down_h0)])
    cross_peak_down_neg20_ks,cross_peak_down_neg20_pvalue=stats.ks_2samp(cross_delta_peak_down_neg20[np.isfinite(
        cross_delta_peak_down_neg20)],non_delta_peak_down_neg20[np.isfinite(non_delta_peak_down_neg20)])
    cross_peak_down_wbzh_ks,cross_peak_down_wbzh_pvalue=stats.ks_2samp(cross_delta_peak_down_wbzh[np.isfinite(
        cross_delta_peak_down_wbzh)],non_delta_peak_down_wbzh[np.isfinite(non_delta_peak_down_wbzh)])
    cross_peak_down_1km_srh_ks,cross_peak_down_1km_srh_pvalue=stats.ks_2samp(cross_delta_peak_down_1km_srh[np.isfinite(
        cross_delta_peak_down_1km_srh)],non_delta_peak_down_1km_srh[np.isfinite(non_delta_peak_down_1km_srh)])
    cross_peak_down_3km_srh_ks,cross_peak_down_3km_srh_pvalue=stats.ks_2samp(cross_delta_peak_down_3km_srh[np.isfinite(
        cross_delta_peak_down_3km_srh)],non_delta_peak_down_3km_srh[np.isfinite(non_delta_peak_down_3km_srh)])
    cross_peak_down_1km_shear_ks,cross_peak_down_1km_shear_pvalue=stats.ks_2samp(cross_delta_peak_down_1km_shear[np.isfinite(
        cross_delta_peak_down_1km_shear)],non_delta_peak_down_1km_shear[np.isfinite(non_delta_peak_down_1km_shear)])
    cross_peak_down_3km_shear_ks,cross_peak_down_3km_shear_pvalue=stats.ks_2samp(cross_delta_peak_down_3km_shear[np.isfinite(
        cross_delta_peak_down_3km_shear)],non_delta_peak_down_3km_shear[np.isfinite(non_delta_peak_down_3km_shear)])
    cross_peak_down_6km_shear_ks,cross_peak_down_6km_shear_pvalue=stats.ks_2samp(cross_delta_peak_down_6km_shear[np.isfinite(
        cross_delta_peak_down_6km_shear)],non_delta_peak_down_6km_shear[np.isfinite(non_delta_peak_down_6km_shear)])
    cross_peak_down_sbcape_ks,cross_peak_down_sbcape_pvalue=stats.ks_2samp(cross_delta_peak_down_sbcape[np.isfinite(
        cross_delta_peak_down_sbcape)],non_delta_peak_down_sbcape[np.isfinite(non_delta_peak_down_sbcape)])
    cross_peak_down_mlcape_ks,cross_peak_down_mlcape_pvalue=stats.ks_2samp(cross_delta_peak_down_mlcape[np.isfinite(
        cross_delta_peak_down_mlcape)],non_delta_peak_down_mlcape[np.isfinite(non_delta_peak_down_mlcape)])
    cross_peak_down_mucape_ks,cross_peak_down_mucape_pvalue=stats.ks_2samp(cross_delta_peak_down_mucape[np.isfinite(
        cross_delta_peak_down_mucape)],non_delta_peak_down_mucape[np.isfinite(non_delta_peak_down_mucape)])
    cross_peak_down_sbcin_ks,cross_peak_down_sbcin_pvalue=stats.ks_2samp(cross_delta_peak_down_sbcin[np.isfinite(
        cross_delta_peak_down_sbcin)],non_delta_peak_down_sbcin[np.isfinite(non_delta_peak_down_sbcin)])
    cross_peak_down_mlcin_ks,cross_peak_down_mlcin_pvalue=stats.ks_2samp(cross_delta_peak_down_mlcin[np.isfinite(
        cross_delta_peak_down_mlcin)],non_delta_peak_down_mlcin[np.isfinite(non_delta_peak_down_mlcin)])
    cross_peak_down_mucin_ks,cross_peak_down_mucin_pvalue=stats.ks_2samp(cross_delta_peak_down_mucin[np.isfinite(
        cross_delta_peak_down_mucin)],non_delta_peak_down_mucin[np.isfinite(non_delta_peak_down_mucin)])
    cross_peak_down_stp_ks,cross_peak_down_stp_pvalue=stats.ks_2samp(cross_delta_peak_down_stp[np.isfinite(
        cross_delta_peak_down_stp)],non_delta_peak_down_stp[np.isfinite(non_delta_peak_down_stp)])
    cross_peak_down_scp_ks,cross_peak_down_scp_pvalue=stats.ks_2samp(cross_delta_peak_down_scp[np.isfinite(
        cross_delta_peak_down_scp)],non_delta_peak_down_scp[np.isfinite(non_delta_peak_down_scp)])
    cross_peak_down_sherb_ks,cross_peak_down_sherb_pvalue=stats.ks_2samp(cross_delta_peak_down_sherb[np.isfinite(
        cross_delta_peak_down_sherb)],non_delta_peak_down_sherb[np.isfinite(non_delta_peak_down_sherb)])
    cross_peak_down_1km_lapse_ks,cross_peak_down_1km_lapse_pvalue=stats.ks_2samp(cross_delta_peak_down_1km_lapse[np.isfinite(
        cross_delta_peak_down_1km_lapse)],non_delta_peak_down_1km_lapse[np.isfinite(non_delta_peak_down_1km_lapse)])
    cross_peak_down_2km_lapse_ks,cross_peak_down_2km_lapse_pvalue=stats.ks_2samp(cross_delta_peak_down_2km_lapse[np.isfinite(
        cross_delta_peak_down_2km_lapse)],non_delta_peak_down_2km_lapse[np.isfinite(non_delta_peak_down_2km_lapse)])
    cross_peak_down_3km_lapse_ks,cross_peak_down_3km_lapse_pvalue=stats.ks_2samp(cross_delta_peak_down_3km_lapse[np.isfinite(
        cross_delta_peak_down_3km_lapse)],non_delta_peak_down_3km_lapse[np.isfinite(non_delta_peak_down_3km_lapse)])
    cross_peak_down_6km_lapse_ks,cross_peak_down_6km_lapse_pvalue=stats.ks_2samp(cross_delta_peak_down_6km_lapse[np.isfinite(
        cross_delta_peak_down_6km_lapse)],non_delta_peak_down_6km_lapse[np.isfinite(non_delta_peak_down_6km_lapse)])
    cross_peak_down_mid_rh_ks,cross_peak_down_mid_rh_pvalue=stats.ks_2samp(cross_delta_peak_down_mid_rh[np.isfinite(
        cross_delta_peak_down_mid_rh)],non_delta_peak_down_mid_rh[np.isfinite(non_delta_peak_down_mid_rh)])
    cross_peak_down_sfc_theta_ks,cross_peak_down_sfc_theta_pvalue=stats.ks_2samp(cross_delta_peak_down_sfc_theta[np.isfinite(
        cross_delta_peak_down_sfc_theta)],non_delta_peak_down_sfc_theta[np.isfinite(non_delta_peak_down_sfc_theta)])
    cross_peak_down_ml_theta_ks,cross_peak_down_ml_theta_pvalue=stats.ks_2samp(cross_delta_peak_down_ml_theta[np.isfinite(
        cross_delta_peak_down_ml_theta)],non_delta_peak_down_ml_theta[np.isfinite(non_delta_peak_down_ml_theta)])
    cross_peak_down_dcape_ks,cross_peak_down_dcape_pvalue=stats.ks_2samp(cross_delta_peak_down_dcape[np.isfinite(
        cross_delta_peak_down_dcape)],non_delta_peak_down_dcape[np.isfinite(non_delta_peak_down_dcape)])
    
    
    
#Calculating the 2 sample t test stats and p-values 
    #Upstream
    up_lcl_t,up_lcl_pvalue_t=(stats.ttest_ind(cross_up_lcl[np.isfinite(cross_up_lcl)], 
                                            non_up_lcl[np.isfinite(non_up_lcl)]))
    up_lfc_t,up_lfc_pvalue_t=(stats.ttest_ind(cross_up_lfc[np.isfinite(cross_up_lfc)], 
                                            non_up_lfc[np.isfinite(non_up_lfc)]))
    up_el_t,up_el_pvalue_t=(stats.ttest_ind(cross_up_el[np.isfinite(cross_up_el)], 
                                          non_up_el[np.isfinite(non_up_el)]))
    up_h0_t,up_h0_pvalue_t=(stats.ttest_ind(cross_up_h0[np.isfinite(cross_up_h0)],
                                          non_up_h0[np.isfinite(non_up_h0)]))
    up_h_neg20_t,up_h_neg20_pvalue_t=(stats.ttest_ind(cross_up_h_neg20[np.isfinite(cross_up_h_neg20)], 
                                                    non_up_h_neg20[np.isfinite(non_up_h_neg20)]))
